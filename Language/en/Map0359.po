msgid ""
"\\>\\c[5]Matrose:\\c[0]\n"
"Ihr elenden Monster !\n"
"\\.\\.Jetzt werdet ihr sterben !"
msgstr ""
"\\>\\c[5]Sailor:\\c[0]\n"
"You wretched monsters!\n"
"\\.\\.Now you will die!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Sind alle Matrosen so naiv ?"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Are all sailors so naïve?"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Dein Übermut ist nun dein Tod !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Your overconfidence was your death!"

msgid ""
"\\>\\c[11]Matrose:\\c[0]\n"
"Was haben wir denn hier ?\n"
"\\.\\.Blinde Passagiere !"
msgstr ""
"\\>\\c[11]Sailor:\\c[0]\n"
"What do we have here?\n"
"\\.\\.Stowaways!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Matrosen sind auch nicht mehr\n"
"das, was sie mal waren ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Sailors are also no longer what they\n"
"used to be..."

msgid ""
"\\>\\c[8]Matrose:\\c[0]\n"
"Was seid ihr ?"
msgstr ""
"\\>\\c[8]Sailor:\\c[0]\n"
"What are you?"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Dein schlimmster Alptraum !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Your worst nightmare!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Und aus diesem Alptraum wirst du\n"
"nie wieder erwachen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"And it's a nightmare you will never\n"
"awake from!"

msgid ""
"\\>\\c[\\v[436]]\\n[\\v[816]]:\\c[0]\n"
"Soll ich mich in eine \n"
"Fledermaus verwandeln?"
msgstr ""
"\\>\\c[\\v[436]]\\n[\\v[816]]:\\c[0]\n"
"Shall I turn into a bat ?"

msgid ""
"Ja.\n"
"Nein."
msgstr ""
"Yes.\n"
"No."

