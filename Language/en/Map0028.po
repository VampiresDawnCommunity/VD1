msgid ""
"\\>\\c[6]Alte Krauthexe:\\c[0]\n"
"\\S[2]Ach, da wird einem doch wirklich\n"
"kräutrig ums raue Hexenherz zumute...\n"
"wenn die eigenen Enkeltöchter wie man"
msgstr ""

msgid ""
"\\>\\c[6]Alte Krauthexe:\\c[0]\n"
"\\S[2]selbst eins vor vielen Sommern\n"
"die Kelle schwingen und widerwärtige\n"
"Tränke brauen, um sie den "
msgstr ""

msgid ""
"\\>\\c[6]Alte Krauthexe:\\c[0]\n"
"\\S[2]aufgeblähtesten Menschen unter\n"
"die Kartoffelsuppe zu mischen...\n"
"Ich bin so stolz."
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"Iff haf Angft vor Fremden. Wenn \n"
"ihr waff wollt, ffrefft mit meiner\n"
"groffen Ffeffta! "
msgstr ""

msgid ""
"\\>\\c[6]Sehr junge Krauthexe:\\c[0]\n"
"Iff haff von fehlrt, ihr wollt Ftränke\n"
"kaufen? Gute Enffeidung!! "
msgstr ""

msgid ""
"\\>\\c[6]Sehr junge Krauthexe:\\c[0]\n"
"Wir maffen daff fen fanfen Taff! \n"
"Eff macht Fpaff und man lernft \n"
"waff dabei!!"
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"Diese Welt ist so schön, vor allem,\n"
"da es hier so gut wie keine anderen\n"
"Hexen gibt. \\.\\.Keine Konkurrenz, hihi."
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Und was genau tut IHR hier direkt\n"
"neben meinem Schloss mit diesem\n"
"riesigen Kessel? \\.\\.Das Zeug stinkt!"
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"Un\\.\\.-hö\\.\\.-flich!\\.\\.\n"
"Wir brauen einen kräftigen Trunk für\n"
"eure Truppen, um sie ein wenig, äh,"
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"\\.\\.\\.\\.effektiver zu machen.\n"
"Das war das Wort, das ich gesucht\n"
"habe."
msgstr ""

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Ich übersetze das mal: \\.\\.\n"
"Ihr experimentiertan ihnen, um euer\n"
"Gebräu zu testen und noch effektiver"
msgstr ""

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"zu machen."
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"N-\\.\\.Na ja, kann sein, aber ist das was\n"
"Schlechtes?\\.\\. Immerhin trinken sie \n"
"es gern, es schmeckt super! "
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Es ist was Schlechtes, wenn der Geruch\n"
"Menschen anlockt und meine Wachen\n"
"davon von den Zinnen fallen."
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"Wird nicht passieren, ganz hohes \n"
"Hexenversprechen! Und damit ihr\n"
"euch von unserem Nutzen überzeugen"
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"könnt, solltet ihr bei meiner Schwester\n"
"einige unserer Erzeugnisse erstehen."
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Für GELD?"
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"Äh, \\.\\.hehehehe...\\.\\.\\.\\.\n"
"Die Zutaten bezahlen sich nicht\n"
"von selbst, und auf diesem kahlen Berg"
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"findet man nichts, also muss ich\n"
"auf meinem Besen in Städte\n"
"reisen um einzukaufen..."
msgstr ""

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Die moderne Hexe kauft..."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Das ist für eine Hexe ziemlich\n"
"ernüchternd..."
msgstr ""

msgid ""
"\\>\\c[6]Junge Krauthexe:\\c[0]\n"
"Unsere Tränke werden toll wirken,\n"
"und ihr könnt sie gleich bei meiner\n"
"Schwester erstehen!"
msgstr ""

msgid ""
"\\>\\c[1]Dämon oder so:\\c[0]\n"
"Aus dem Weg, Sire!\\.\\.\n"
"Mein Pferd ist ähnlich tollpatschig\n"
"wie das eines Hexers, also "
msgstr ""

msgid ""
"\\>\\c[1]Dämon oder so:\\c[0]\n"
"fordert es nicht heraus, sonst segle\n"
"ich wie ein Felsen die Klippen\n"
"herunter! BRRRR!!!!"
msgstr ""

msgid ""
"\\>\\c[6]Skelettritter:\\c[0]\n"
"Die Skelette im Truppenraum unten\n"
"machen mir Angst\\..\\..\\..\\.\\.\n"
"Die werfen mirt Blicke zu, "
msgstr ""

msgid ""
"\\>\\c[6]Skelettritter:\\c[0]\n"
"die ich noch aus meiner Zeit als\n"
"Fleischträger kenne."
msgstr ""

msgid ""
"\\>\\c[6]Hauptmann Varianis:\\c[0]\n"
"Ein schöner Tag, um unsere \n"
"Gefolgsleute herumzuscheuchen,\n"
"findet Ihr nicht auch, Herr?"
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Da triffst du bei mir genau den\n"
"richtigen Nerv!! Lass sie ordentlich\n"
"bluten."
msgstr ""

msgid ""
"\\>\\c[6]Hauptmann Varianis:\\c[0]\n"
"Ein Tag ist nicht vollkommen,\n"
"bis nicht auch die Skelette und\n"
"Geister ausbluten."
msgstr ""

msgid "Menschlichkeit um 1 gesunken!"
msgstr ""

