msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ah, da steht jemand.\n"
"Oder ETWAS."
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]Was?!"
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[3]Die noch lebenden Vampire\\..\\..\\..\n"
"Ihr seid also hier. "
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Kennen wir uns?!"
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]Nein.\\.\\.\n"
"Noch nicht jedenfalls.\\.\\.\n"
"Aber ich weiß, wer ihr seid."
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]Und was ihr getan habt.\n"
"Glückwunsch."
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Sprichst du, wovon ich denke,\n"
"dass du sprichst?!"
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]Der Schöpfer, der seiner\n"
"Schöpfung zum Opfer fiel..."
msgstr ""

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Wer BIST du, und woher weißt\n"
"du das alles?!"
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]Nun, ich habe euch\\..\\..\\..\n"
"und alle anderen Vampire dieser\n"
"Welt beobachtet, und das schon"
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]länger. Es überrascht mich also\n"
"nicht, dass ihr irgendwie einen Weg\n"
"in mein Versteck gefunden habt..."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Bist du den Vampiren feindlich oder\n"
"freundlich gesinnt?"
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]Ich würde sagen, das hängt ganz\n"
"davon ab, was ihr noch für mich tun\n"
"könnt... aber wer Vincent Weynard "
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]in die Knie zwingt, den werde\n"
"ich hier und heute sicherlich\n"
"nicht abschlachten, keine Sorge."
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Du klingst ziemlich überheblich für\n"
"einen Clown in einem Loch."
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]Vielleicht ist es an der Zeit,\n"
"euer Potential einmal mit eigener\n"
"Hand zu testen. Es gäbe schließlich"
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]nichts, das mein Gedächtniszauber\n"
"nicht wieder unter Kontrolle\n"
"brächte\\..\\..\\.."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Was willst du, du Hofnarren-\n"
"Verschnitt?!"
msgstr ""

msgid ""
"\\>\\c[12]?:\\c[0]\n"
"\\S[2]Euren SIEG."
msgstr ""

