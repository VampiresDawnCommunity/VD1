msgid ""
"\\>\\c[10]Stimme:\\c[0]\n"
"HARHAR! Versucht nur, durch mein\n"
"tödliches Labyrinth zu kommen,\n"
"ihr Diebe!"
msgstr ""
"\\c[10]Voice:\\c[0]\n"
"HARHAR! Try getting through my deadly\n"
"maze, you thieves!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ugh.\\.\\. Passt auf, wo ihr hintretet!"
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"Ugh.\\.\\. Watch your steps!"

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Schaut genauer hin! Er hat seine\n"
"Fallen nicht gut versteckt."
msgstr ""
"\\c[4]Alaine:\\c[0]\n"
"Look closely! He didn't do a good\n"
"job hiding the traps.\\.\\."

