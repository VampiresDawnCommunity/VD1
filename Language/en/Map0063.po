msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"ihn benutzte."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"me by that name..."

#. ID 21, Page 9, Line 356, Pos (0,8)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Bitte nenne mich weiter Aysha.\n"
"\\.\\.Das ist Teil meines neuen Lebens."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Please keep calling me Aysha.\n"
"\\.\\.It's part of my new life."

#. ID 21, Page 9, Line 361, Pos (0,8)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"In Ordnung, Aysha."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"All right, Aysha."

