msgid ""
"\\>\\c[10]Kapitän:\\c[0]\n"
"Aye, was seid ihr denn für blinde\n"
"Passagiere ?\n"
"\\.\\.Dafür werdet ihr bezahlen !"
msgstr ""
"\\>\\c[10]Captain:\\c[0]\n"
"Aye, what kind of stowaways are you?\n"
"\\.\\.You're going to pay for that!"

msgid ""
"\\>\\c[5]Piratenvorsitz:\\c[0]\n"
"Aye, was seid ihr denn für blinde\n"
"Passagiere ?\n"
"\\.\\.Dafür werdet ihr bezahlen !"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Nein, der Einzige, der hier etwas\n"
"bezahlt bist DU !\n"
"\\.\\.Und zwar dein Leben !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"No, the only one who is going to pay\n"
"is YOU!\n"
"\\.\\.With your life!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Und wieder einer weniger !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"And another one gone!"

