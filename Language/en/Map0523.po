msgctxt "actors.name"
msgid "Linar"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Keines dieser Bücher sieht\n"
"interessant aus."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"None of these books sound\n"
"interesting."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hey!\\.\\. Diese Bücher sind nur\n"
"Attrappen!"
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"Hey!\\.\\. These books are fake!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Warte.\\.\\. Da hinten in der Ecke ist\n"
"ein echtes versteckt."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"Wait.\\.\\. There's a real one hidden\n"
"in the corner."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\..\\..\\.."
msgstr ""

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Was?"
msgstr ""
"\\c[4]Alaine:\\c[0]\n"
"What?"

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"\\.\\.Obwohl, ich will es gar nicht\n"
"wissen."
msgstr ""
"\\c[4]Alaine:\\c[0]\n"
"\\.\\.Actually, I don't want to know."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich wünschte, ich hätte nicht\n"
"reingeschaut ..."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"I wish I hadn't looked..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Da werde ich nicht noch einmal\n"
"reinschauen."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"I'm not going to look again."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Dr. Man Tu Chi - Erster Entwurf\" und\n"
"\"Glamouröser Aufstieg\" stehen hier im\n"
"Regal."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"\"Dr. Man Tu Chi - First Draft\", and\n"
"\"Glamorous Ascension\" are on this\n"
"shelf."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Meine guten Taten\"."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"\"My Good Deeds\"."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, alle Seiten sind leer..."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"Hm, all pages are empty..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Eine große Sammlung von Wissen\n"
"über Bären."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"A great collection of knowledge about\n"
"the bear species."

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Süß."
msgstr ""
"\\c[4]Alaine:\\c[0]\n"
"Cute."

