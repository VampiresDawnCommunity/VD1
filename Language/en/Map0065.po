msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"time."

#. ID 21, Page 5, Line 232, Pos (20,21)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Das ist gut ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Sounds good..."

#. ID 21, Page 5, Line 235, Pos (20,21)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Also pass auf, die Zauber\n"
"gehen so ..."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Okay, now pay attention, this is how\n"
"the spells work..."

#. ID 21, Page 5, Line 242, Pos (20,21)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Aha ... \\.\\.ja ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Aha... \\.\\.yes..."

#. ID 21, Page 5, Line 247, Pos (20,21)
msgid ""
"\n"
"     \"Fledermausmantel\" und \"Zuflucht\" erlernt !"
msgstr ""
"\n"
"        \"Bat Cloak\" and \"Retreat\" learned!"

#. ID 21, Page 5, Line 259, Pos (20,21)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Ich gebe dir noch eine Karte,\n"
"mit der du dich besser auf der Welt\n"
