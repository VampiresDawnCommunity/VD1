msgid "FlugBla"
msgstr ""

msgid ""
"\\>\\c[6]???:\\c[0]\n"
"\\S[4]Die Schreie eures Magierzimmers\\..\\..\\..\n"
"Sie sind bis hier zu hören. Meine Ohren\\..\\..\\..\n"
"Und meine Seele reagieren auf sie."
msgstr ""

msgid ""
"\\>\\c[6]???:\\c[0]\n"
"\\S[4]Doch weder sie.\\..\\..\\..\n"
"noch die Blutgeister, die euer \n"
"Schicksal sind, machen sich "
msgstr ""

msgid ""
"\\>\\c[6]???:\\c[0]\n"
"\\S[4]eine Vorstellung von ihrem Glück.\\..\\..\\..\n"
"Die HÖLLE\\| ist ein Ort unaussprechlichen\n"
"Grauens, das eure Vorstellung schmelzt."
msgstr ""

msgid ""
"\\>\\c[6]???:\\c[0]\n"
"\\S[4]Ich kam\\.\\.\\. als ihr Gefangener.\\.\\.\n"
"Ich ging\\.\\.\\. als ihr Knecht.\\.\\.\n"
"Nun diene ich den Untoten."
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wer weiß schon, was Menschen wirklich\n"
"erwartet, sobald ihr Leben endet? Sollten\n"
"sie wirklich an einem Ort namens 'Hölle'"
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"leiden, beweist das doch nur ihre \n"
"Unterlegenheit uns Vampiren \n"
"gegenüber\\..\\..\\.."
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Aber dann auch wieder bringt\n"
"eine Hölle einen Gott oder einen\n"
"Teufel mit sich. Und die GIBT es nicht."
msgstr ""

msgid ""
"\\>\\c[6]???:\\c[0]\n"
"\\S[4]Hoffet, dass unendliche Qualen \n"
"auf immer das Schlimmste sein m\n"
"mögen, dass euch droht. "
msgstr ""

msgid ""
"\\>\\c[6]???:\\c[0]\n"
"\\S[4]Und betet, oh betet so, dass\n"
"euer Unwissen nicht unumstößlich\n"
"korrigiert werden möge."
msgstr ""

msgid ""
"\\>\\c[6]???:\\c[0]\n"
"\\S[4]Die HÖLLE\\.\\.\\. ist real. \\.\\.\\.\n"
"Und ob man an sie glaubt oder nicht -\\.\\.\\.\n"
"Man fühlt sie."
msgstr ""

