#. ID 21, Page 1, Line 2, Pos (9,78)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"Jetzt, da ich wieder in eine\n"
"Welt beschworen wurde, habe ich\n"
"erneut die Chance, irgendwann, "
msgstr ""

#. ID 21, Page 1, Line 6, Pos (9,78)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"wenn meine Dienste für Euch \n"
"beendet sind, vielleicht\\..\\..\\.\\.\\.\n"
"eine Familie zu gründen."
msgstr ""

#. ID 21, Page 1, Line 11, Pos (9,78)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Bitte?! Du, ein Dämon,\n"
"mit einer Familie?"
msgstr ""

#. ID 21, Page 1, Line 15, Pos (9,78)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"Spottet nur Gebieter, aber auch\n"
"uns magische Wesen zieht es \n"
"zuweilen zu solch menschlichen"
msgstr ""

#. ID 21, Page 1, Line 19, Pos (9,78)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"Bedürfnissen. Ich träume schon \n"
"seit Jahrhunderten davon, doch\n"
"stets bin ich zu früh verstorben..."
msgstr ""

#. ID 21, Page 1, Line 24, Pos (9,78)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Ich finde das überhaupt nicht albern!\\.\\.\n"
"Über eine Familie habe ich auch\n"
"schon öfter nachgedacht. Es spielt "
msgstr ""

#. ID 21, Page 1, Line 28, Pos (9,78)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"doch gar keine Rolle, \n"
"WAS du bist..."
msgstr ""

#. ID 21, Page 1, Line 34, Pos (9,78)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Wir verstehen diese Gedanken\n"
"auch, nicht wahr, Valnar?"
msgstr ""

#. ID 21, Page 1, Line 38, Pos (9,78)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ja, und das als Vampire...\n"
"Hab keine Zweifel. In so \n"
"einem Leben braucht man Ziele."
msgstr ""

#. ID 21, Page 1, Line 45, Pos (9,78)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Als Vampir verstehe ich dich...\n"
"Hab keine Zweifel. In so \n"
"einem Leben braucht man Ziele."
msgstr ""

#. ID 21, Page 1, Line 52, Pos (9,78)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"Vielen Dank für Eure Worte, \n"
"Gebieter\\..\\..\\.\\.\\.\n"
"Das bedeutet mir viel."
msgstr ""

#. ID 21, Page 1, Line 56, Pos (9,78)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"Diese Welt erscheint friedlich,\n"
"und wir sind zahlreich. Meine\n"
"Chancen stehen nicht schlecht."
msgstr ""

#. ID 21, Page 1, Line 61, Pos (9,78)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Viel Erfolg..."
msgstr ""

#. ID 21, Page 2, Line 2, Pos (9,78)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"Für meine zukünftige Familie\n"
"werde ich mich nicht in unnötige\n"
"Kämpfe stürzen."
msgstr ""

#. ID 21, Page 2, Line 7, Pos (9,78)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Das ist sehr vernünftig von dir,\n"
"du wirst dein Ziel sicher erreichen."
msgstr ""

#. ID 21, Page 2, Line 11, Pos (9,78)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"Ich weiß Eure Worte zu\n"
"schätzen, Gebieterin."
msgstr ""

#. ID 29, Page 1, Line 2, Pos (7,65)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"\\S[3]Oooooooh, eine Lamia! \n"
"Die kenne ich nur aus meinen\n"
"Märchenbüchern. Und dann auch"
msgstr ""

#. ID 29, Page 1, Line 6, Pos (7,65)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"\\S[3]noch eine so Wunderschöne\n"
"dazu!"
msgstr ""

#. ID 29, Page 1, Line 12, Pos (7,65)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"\\S[2]Bist du süüüß...\n"
"Deine kleinen, blauen Schuppen...\n"
"Wie niedlich!"
msgstr ""

#. ID 29, Page 1, Line 19, Pos (7,65)
msgid ""
"\\>\\c[6]Lamia:\\c[0]\n"
"Ähm, Entschuldigung?"
msgstr ""

#. ID 29, Page 1, Line 22, Pos (7,65)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"\\S[3]Du musst dich für gar nichts\n"
"entschuldigen! Darf ich dich mal \n"
"streicheln?"
msgstr ""

#. ID 29, Page 1, Line 29, Pos (7,65)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"\\S[2]Oder vielleicht deinen \n"
"kleinen Schwanz anfassen?\n"
"Der ist so beweglich..."
msgstr ""

#. ID 29, Page 1, Line 37, Pos (7,65)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\S[5].............."
msgstr ""

#. ID 29, Page 1, Line 40, Pos (7,65)
msgid ""
"\\>\\c[6]Lamia:\\c[0]\n"
"\\.\\.\\. Ich gehe jetzt besser, ehe\\..\\..\\..\n"
"hier noch Unaussprechliches passiert.\\.\\.\n"
"Mit mir. "
msgstr ""

#. ID 29, Page 1, Line 45, Pos (7,65)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"\\S[3]Ach schade, lass uns doch\n"
"wenigstens einmal kurz kuscheln..."
msgstr ""

#. ID 29, Page 1, Line 51, Pos (7,65)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"\\S[2]Du siehst so unglaublich\n"
"weich aus!"
msgstr ""

#. ID 29, Page 1, Line 60, Pos (7,65)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\S[4]Es tut uns leeeeeid!"
msgstr ""

#. ID 29, Page 2, Line 2, Pos (7,65)
msgid ""
"\\>\\c[6]Lamia:\\c[0]\n"
"Ich bin KEIN Kuscheltier!"
msgstr ""

#. ID 29, Page 2, Line 5, Pos (7,65)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"\\S[3]Na, das finden wir irgendwann\n"
"noch heraus."
msgstr ""

#. ID 29, Page 2, Line 11, Pos (7,65)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"\\S[2]Ganz genau, Schwester."
msgstr ""

#. ID 29, Page 2, Line 16, Pos (7,65)
msgid ""
"\\>\\c[6]Lamia:\\c[0]\n"
"\\.\\.\\.\\.\\.\\."
msgstr ""

#. ID 30, Page 1, Line 2, Pos (9,49)
msgid ""
"\\>\\c[6]Dämon:\\c[0]\n"
"Ich ZERSCHMETTERE Eure Feinde\n"
"mit dunkler Magie und wirklich\n"
"FIESEN Absichten! "
msgstr ""
"\\>\\c[6]Demon:\\c[0]\n"
"I'll SMASH your enemies with\n"
"dark magic and really\n"
"NASTY intentions!"

