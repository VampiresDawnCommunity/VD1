msgid "Default"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"So, und nun her mit dem\n"
"guten Stück !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"All right, now, let's take this thing!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Valnar, du bist ein Weichei !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Valnar, you are a wimp!"

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Sehr gut !"
msgstr ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Excellent!"

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Das wollte ich hören."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"That's what I wanted to hear."

msgid ""
"\\>\\c[11]Golbar:\\c[0]\n"
"HIER ER SAIN !"
msgstr ""
"\\>\\c[11]Golbar:\\c[0]\n"
"HERE IT IS!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Der Kerl in Klennar merkt den\n"
"Unterschied bestimmt nicht !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Surely, the guy in Klennar will not\n"
"notice the difference!"

msgid ""
"\\>\\c[11]Golbar:\\c[0]\n"
"IHRR NUN BITTÄ GEHÄN !"
msgstr ""
"\\>\\c[11]Golbar:\\c[0]\n"
"PLEASE YOU LEAVE NOW!"

msgid ""
"\\>\\c[11]Golbar:\\c[0]\n"
"WASSS WOLLÄN VON GOLBAR?"
msgstr ""
"\\c[11]Golbar:\\c[0]\n"
"WHAT WANT FROM GOLBAR?"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir sollen dir diese Steintafel\n"
"von jemanden namens Guntra\n"
"bringen."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"We're supposed to give you this\n"
"stone tablet from someone named\n"
"Guntra."

msgid ""
"\\>\\c[11]Golbar:\\c[0]\n"
"GUNTRA!"
msgstr ""
"\\c[11]Golbar:\\c[0]\n"
"GUNTRA!"

msgid ""
"\\>\\c[11]Golbar:\\c[0]\n"
"\\s[4]...\\s[0]"
msgstr ""
"\\c[11]Golbar:\\c[0]\n"
"\\s[4]...\\s[0]"

msgid ""
"\\>\\c[11]Golbar:\\c[0]\n"
"\\.\\.Ja! Golbar gehän! Golbar bereit ..."
msgstr ""
"\\c[11]Golbar:\\c[0]\n"
"\\.\\.Yes! Golbar go! Golbar ready..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ähm ..."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"Uhm..."

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Lasst uns zurück zum Wald gehen!\n"
"\\.\\.Ich möchte sehen, wie es weitergeht."
msgstr ""
"\\c[4]Alaine:\\c[0]\n"
"Let's return to the forest!\n"
"\\.\\.I want to see what happens next."

