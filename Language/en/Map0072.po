msgid ""
"\\>\\c[6]Gletcherork:\\c[0]\n"
"Mir warme Schlosshallen\n"
"von Vampiren viel lieber als\n"
"kalte Eiswälder sein..."
msgstr ""

msgid ""
"\\>\\c[6]Orkengardist:\\c[0]\n"
"Wir sind die stärksten Orks\n"
"des Königs! Falls ihr mir nicht\n"
"glaubt, sprecht mit dem Hauptmann!"
msgstr ""

msgid ""
"\\>\\c[6]Elite-Skelett:\\c[0]\n"
"Der Hauptmann hat eine\n"
"angeborene Abneigung \n"
"gegen Vampire, seit seine"
msgstr ""

msgid ""
"\\>\\c[6]Elite-Skelett:\\c[0]\n"
"Frau von einem verwandelt \n"
"wurde und ihn... na ja, abgemurkst\n"
"hat. Seht es ihm nach."
msgstr ""

msgid ""
"\\>\\c[6]Untoter Barbar:\\c[0]\n"
"\\S[4]Diese Welt... ist viel... viel...\n"
"VIEL zu friedlich...\n"
"Ich will BLUT sehen..."
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wie ich dieses Gefühl kenne..."
msgstr ""

msgid ""
"\\>\\c[6]Skelettkrieger:\\c[0]\n"
"So ganz ohne Muskeln ist es \n"
"schwer, wirklich kräftig \n"
"zuzuschlagen, aber dafür haben"
msgstr ""

msgid ""
"\\>\\c[6]Skelettkrieger:\\c[0]\n"
"die meisten von uns Gewichte\n"
"an den Knochen angebracht..."
msgstr ""

msgid ""
"\\>\\c[6]Skelettmagier:\\c[0]\n"
"Falls Ihr gegen uns trainieren\n"
"solltet, werter Gebieter, gebe\n"
"ich Euch einen Tipp..."
msgstr ""

msgid ""
"\\>\\c[6]Skelettmagier:\\c[0]\n"
"Licht und Schattenmagie \n"
"funktioniert gegen uns nicht.\n"
"Dafür habe ich gesorgt."
msgstr ""

msgid ""
"\\>\\c[6]Zombiekrieger:\\c[0]\n"
"\\S[2]Diese verflixten Orks \n"
"starren uns an wie ihr \n"
"Mittagessen... "
msgstr ""

msgid ""
"\\>\\c[6]Zombiekrieger:\\c[0]\n"
"\\S[2]Sollen sie nur kommen!\n"
"Wir schmecken eh nach\n"
"Schimmeltorte."
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Keine Kämpfe unter meinen\n"
"Dienern, klar?"
msgstr ""

msgid ""
"\\>\\c[6]Zombiekrieger:\\c[0]\n"
"\\S[2]Sagt das denen, Gebieter..."
msgstr ""

msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"\\S[3]Diese verlausten Orks\n"
"stinken bis hier runter..."
msgstr ""

msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"Unser Kommandant ist im \n"
"Herzogtum, aus dem wir \n"
"kommen, der stärkste aller"
msgstr ""

msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"Ghouls...\n"
"Na ja, vielleicht bis auf \n"
"den Herzog. "
msgstr ""

msgid ""
"\\>\\c[6]Kriegsgeist:\\c[0]\n"
