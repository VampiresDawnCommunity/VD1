msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Endlich wieder hier .\\..\\..\\.\n"
"Jetzt schlafe ich erst einmal."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Finally back\\..\\..\\..\n"
"Now it's time for me to rest."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[4]Ich habe Hunger !!!\\s[0]"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[4]I am hungry!!!\\s[0]"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Jetzt nicht !\n"
"Ich bin nicht müde."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Not now! I'm not tired anymore."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich will nichts von Asgars Sarg !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I don't want aything from Asgar's\n"
"coffin!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich will nichts von Alaines Sarg !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I don't want aything from Alaine's\n"
"coffin!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Ah, meine Liebe !\n"
"\\.\\.Er kommt zu sich !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Ah, my love!\n"
"\\.\\.He's regaining consciousness!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Nimm diese Brille ...\n"
"\\.\\.Du wirst jetzt etwas\n"
"empfindlich gegen Licht sein."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Take these shades...\n"
"\\.\\.You are now a little sensitive to\n"
"light."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Willkommen in meiner Welt,\n"
"\\s[8]Valnar !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Welcome to my world,\n"
"\\s[8]Valnar!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\s[6]NEIIIIIIIIIIIIIIIN !!!!!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\s[6]NOOOOOOOOOO!!!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\s[6].............."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\s[6].............."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\s[6]Wie konntest du es wagen ???"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\s[6]How dare you!?"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"HARHAR !\n"
"\\.\\.Du fragst mich, wie ich es\n"
"\\s[6]wagen\\s[0] konnte ?"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"HARHAR!\n"
"\\.\\.You're asking me how I\n"
"\\s[6]dare\\s[0]?"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Ich sage dir wie !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"I tell you how!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"DU .\\..\\.."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"YOU\\..\\..\\.."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"GEHÖRST \\..\\..\\.."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"ARE\\..\\..\\.."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"MIR !!!!"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"MINE!!!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Du bist von nun an\n"
"MEIN EIGENTUM !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"You are now MY POSSESSION!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Und solltest du es jemals wagen,\n"
"meine Autorität in Frage zu\n"
"stellen, \\s[4]wirst du sterben !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"And should you ever question my\n"
"authority, \\s[4]you will die!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Und nun gebe ich dir etwas\n"
"Zeit zum Nachdenken !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"And now I'm going to grant you some\n"
"time to think!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Nei ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Noo..."

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Du hattest deinen Spaß...\n"
"Nun habe ich meinen."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"I believe you had your fun for long\n"
"enough, don't you think?"

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Es ist aber zu schade\\..\\..\\..\n"
"\\.\\.\\.\\.\\.\\.\\.Immerhin..."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"This is a bit of a shame, though\\..\\..\\..\n"
"\\.\\.\\.\\.\\.\\.\\.After all..."

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"...wäre mir ein Rotschopf so viel\n"
"lieber gewesen."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"I really would have preferred a\n"
"red-head!"

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"...wäre mir ein Rotschopf so viel\n"
"lieber gewesen.\n"
"\\..\\..\\..Nicht wahr, Marlina?"
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"I really would have preferred a\n"
"red-head!\n"
"\\..\\..\\..Right, Marlina?"

