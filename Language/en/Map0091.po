msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Verdammt !\n"
"\\.\\.\\.Dieser Wald ist ja gigantisch !!!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Damn it!\n"
"\\.\\.\\.This forest is huge!"

msgid "Ts ts ts ..."
msgstr "Ts ts ts..."

msgid "Fliehen tun nur die Schwachen !!!"
msgstr "Only the weak run away!!!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"LASS MICH IN RUHE !!!\n"
"\\.\\.VERSCHWINDE !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"LEAVE ME ALONE!!!\n"
"\\.\\.GET LOST!!!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wie ungezogen ....\n"
"\\.\\.Ich werde dir noch Manieren\n"
"beibringen ...."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"How rude....\n"
"\\.\\.I'll teach you some manners...."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Doch jetzt kommst du erst einmal\n"
"ZURÜCK !!!!"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"But first, you will come BACK!!!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"NIEMALS !!!\n"
"\\.\\.DU BIST EIN TIER !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"NEVER!!!\n"
"\\.\\.YOU ARE A MONSTER!!!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"HARHAR !\n"
"\\.\\.Ich weiß ... \\.\\.Aber du solltest \n"
"wirklich zurückkommen."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"HARHAR!\n"
"\\.\\.I know... \\.\\.But you really should\n"
"come back."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Sonst bin ich gezwungen, den\n"
"Wölfen zu befehlen, dir weh zu tun !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Or else I'll be compelled to make the\n"
"wolves hurt you!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wölfe ??\n"
"\\.\\.WOZU BIN ICH EIN VAMPIR??\n"
"ICH HABE KEINE ANGST !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wolves!?\n"
"\\.\\.I'M A VAMPIRE, AREN'T I!? I'M NOT\n"
"SCARED!!!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Naiv ...\n"
"\\.\\.Wie es zu erwarten war...\n"
"\\.\\.Du bist nur ein junger Vampir ..."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Naïve...\n"
"\\.\\.As to be expected...\n"
"\\.\\.You only are a young vampire..."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"UND ICH BIN DEIN MEISTER !\n"
"\\.\\.Glaubst du etwa, das wären \n"
"normale Wölfe ?"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"AND I'M YOUR MASTER!\n"
"\\.\\.Do you honestly think these are normal\n"
"wolves?"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wo sind deine Wölfe ???\n"
"Ich sehe keine !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Where are your wolves!? I don't see\n"
"any!!!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Aber du kannst sie HÖREN !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"But you can HEAR them!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[9]........"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[9]........"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wir sehen uns Morgen ..."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"I'll see you tomorrow..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"VERDAMMT  !!!\n"
"Wo sind diese Wölfe ?"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"DAMN IT!!! Where are your wolves?"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hau ab du elendes Kötervieh !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Scram, you sordid mutt!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Uffffffffffffff ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ooffffffffffffff..."

