msgctxt "actors.name"
msgid "Pirate"
msgstr ""

msgid ""
"\\>\\c[5]Soldat:\\c[0]\n"
"Dies ist das Schiff von König Gerald !\n"
"\\.\\.Für euer Eindringen werdet ihr\n"
"sterben !"
msgstr ""
"\\>\\c[5]Soldier:\\c[0]\n"
"This is the ship of King Gerald!\n"
"\\.\\.You're going to die for your intrusion!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Tja, nun hat König Gerald einen\n"
"Soldaten weniger !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Well, now King Gerald has one soldier\n"
"less!"

msgid ""
"\\>\\c[11]Soldat:\\c[0]\n"
"Monster !!!!\n"
"\\.\\.Angriff !!!"
msgstr ""
"\\>\\c[11]Soldier:\\c[0]\n"
"Monsters!!! \\.\\.Attack!!!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Und wieder ein Soldat weniger !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"And another soldier gone!"

msgid ""
"\\>\\c[8]Soldat:\\c[0]\n"
"Verflucht !\n"
"\\.\\.Eindringlinge !!!"
msgstr ""
"\\>\\c[8]Soldier:\\c[0]\n"
"Crap! \\.\\.Intruders!!!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Scheint wohl ein wichtiges Schiff zu\n"
"sein, wenn man sofort angegriffen\n"
"wird !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Seems to be a pretty important ship if\n"
"we're getting attacked immediately!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Aber selbst diese Wachen können\n"
"uns nicht aufhalten !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"But even these guards can't stop us!"

msgid ""
"\\>\\c[\\v[436]]\\n[\\v[1546]]:\\c[0]\n"
"Soll ich mich in eine \n"
"Fledermaus verwandeln?"
msgstr ""
"\\>\\c[\\v[436]]\\n[\\v[1546]]:\\c[0]\n"
"Shall I turn into a bat ?"

msgid ""
"Ja.\n"
"Nein."
msgstr ""
"Yes.\n"
"No."

