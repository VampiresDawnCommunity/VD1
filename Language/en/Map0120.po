#. ID 5, Page 1, Line 5, Pos (14,19)
msgid ""
"\\>\\c[4]Mann\\c[0]\n"
"Verschwinde !!!"
msgstr ""
"\\>\\c[4]Man\\c[0]\n"
"Get away!!!"

#. ID 5, Page 1, Line 8, Pos (14,19)
msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"HARHAR !!!\n"
"\\.\\.Ihr werdet sterben !"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"HARHAR!!!\n"
"\\.\\.You are going to die!"

#. ID 5, Page 1, Line 12, Pos (14,19)
msgid ""
"\\>\\c[2]Frau\\c[0]\n"
"Bitte, lass und gehen !\n"
"\\.\\.Wir wollen zu  Vincent Weynard !"
msgstr ""
"\\>\\c[2]Woman\\c[0]\n"
"Please, let us live!\n"
"\\.\\.We want to Vincent Weynard!"

#. ID 5, Page 1, Line 16, Pos (14,19)
msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"ROAAAAAAAR !!!!\n"
"\\.\\.\\s[10]Vincent Weynard !!!\\s[0]"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"ROAAAAAAAR!!!\n"
"\\.\\.\\s[10]Vincent Weynard!!!\\s[0]"

#. ID 5, Page 1, Line 21, Pos (14,19)
msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"Stiefmutter ...\n"
"\\.\\.Wieso hast du ihm das gesagt ?\n"
"\\.\\.ER IST EIN VAMPIR !!!"
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"Stepmother...\n"
"\\.\\.Why did you tell him that?\n"
"\\.\\.HE IS A VAMPIRE!!!"

#. ID 5, Page 1, Line 26, Pos (14,19)
msgid ""
"\\>\\c[2]Frau\\c[0]\n"
"Oh nein !"
msgstr ""
"\\>\\c[2]Woman\\c[0]\n"
"Oh no!"

#. ID 5, Page 1, Line 30, Pos (14,19)
msgid ""
"\\>\\c[4]Mann\\c[0]\n"
"Lass uns in Ruhe, sonst werde ich\n"
"dich aufschlitzen !"
msgstr ""
"\\>\\c[4]Man\\c[0]\n"
"Leave us alone, or I'm going to slice you open!"

#. ID 5, Page 1, Line 36, Pos (14,19)
msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"Niedlich ...\n"
"\\.\\.Und jetzt werdet ihr alle sterben !"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"Cute...\n"
"\\.\\.And now you will all die!"

#. ID 5, Page 1, Line 44, Pos (14,19)
msgid ""
"\\>\\c[4]Mann\\c[0]\n"
"Neiiiiiiiii ..."
msgstr ""
"\\>\\c[4]Man\\c[0]\n"
"Noooooo..."

#. ID 5, Page 1, Line 58, Pos (14,19)
msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"STIEFVATER !!!!!!!!"
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"STEPFATHER!!!"

#. ID 5, Page 1, Line 75, Pos (14,19)
msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"NEIIIIIIIIIIIN !!!!"
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"NOOOOOOOO!!!"

#. ID 5, Page 1, Line 78, Pos (14,19)
msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"HARHAR !!!\n"
"\\.\\.Und jetzt bist du dran !!!"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"HARHAR!!!\n"
"\\.\\.And now it's your turn!!!"

#. ID 5, Page 1, Line 85, Pos (14,19)
msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"Bitte ...."
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"Please...."

#. ID 5, Page 1, Line 90, Pos (14,19)
msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"VAMPIR !!!!"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"VAMPIRE!!!"

#. ID 5, Page 1, Line 95, Pos (14,19)
msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Ich werde dich vernichten !!!"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"I'm going to kill you!!!"

#. ID 5, Page 1, Line 98, Pos (14,19)
msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"\\s[11]Vincent ...\n"
"Vincent Weynard !!!!"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"\\s[11]Vincent...\n"
"Vincent Weynard!!!"

#. ID 5, Page 1, Line 103, Pos (14,19)
msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Du hast dein letztes Opfer gehabt !"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"That was your last victim!"

#. ID 5, Page 1, Line 106, Pos (14,19)
msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"ROAAAAAAR !!!!"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"ROAAAAAAR!!!"

#. ID 5, Page 1, Line 113, Pos (14,19)
msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Dich erwische ich auch noch !!!"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"I'll catch you sooner or later!!!"

#. ID 5, Page 1, Line 117, Pos (14,19)
msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Alles ok ?"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Are you all right?"

#. ID 5, Page 1, Line 121, Pos (14,19)
msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"A \\..\\..\\.. a\\..\\..\\.. alles ok ????\n"
"\\.\\.MEINE STIEFELTERN WURDEN\n"
"GETÖTET !!!!"
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"A\\..\\..\\.. a\\..\\..\\.. all right!?\n"
"\\.\\.MY STEPPARENTS HAVE BEEN KILLED!!!"

#. ID 5, Page 1, Line 126, Pos (14,19)
msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Es tut mir leid, wäre ich doch bloß\n"
"früher gekommen ..."
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"I'm sorry, if only I had arrived\n"
"sooner..."

#. ID 5, Page 1, Line 129, Pos (14,19)
msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Gehe nach Asran, dort wird man\n"
"dir ein Zimmer geben, wenn du\n"
"ihnen sagst, dass ich dich schicke ..."
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Go to Asran, someone will give you a\n"
"room there if you tell them I sent\n"
"you..."

#. ID 5, Page 1, Line 133, Pos (14,19)
msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Es tut mir so leid ..."
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"I am so sorry..."

