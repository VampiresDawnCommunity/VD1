msgid ""
"\\>\\c[4]Mann\\c[0]\n"
"Verschwinde !!!"
msgstr ""
"\\>\\c[4]Man\\c[0]\n"
"Get away!!!"

msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"HARHAR !!!\n"
"\\.\\.Ihr werdet sterben !"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"HARHAR!!!\n"
"\\.\\.You are going to die!"

msgid ""
"\\>\\c[2]Frau\\c[0]\n"
"Bitte, lass und gehen !\n"
"\\.\\.Wir wollen zu  Vincent Weynard !"
msgstr ""
"\\>\\c[2]Woman\\c[0]\n"
"Please, let us live!\n"
"\\.\\.We want to Vincent Weynard!"

msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"ROAAAAAAAR !!!!\n"
"\\.\\.\\s[10]Vincent Weynard !!!\\s[0]"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"ROAAAAAAAR!!!\n"
"\\.\\.\\s[10]Vincent Weynard!!!\\s[0]"

msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"Stiefmutter ...\n"
"\\.\\.Wieso hast du ihm das gesagt ?\n"
"\\.\\.ER IST EIN VAMPIR !!!"
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"Stepmother...\n"
"\\.\\.Why did you tell him that?\n"
"\\.\\.HE IS A VAMPIRE!!!"

msgid ""
"\\>\\c[2]Frau\\c[0]\n"
"Oh nein !"
msgstr ""
"\\>\\c[2]Woman\\c[0]\n"
"Oh no!"

msgid ""
"\\>\\c[4]Mann\\c[0]\n"
"Lass uns in Ruhe, sonst werde ich\n"
"dich aufschlitzen !"
msgstr ""
"\\>\\c[4]Man\\c[0]\n"
"Leave us alone, or I'm going to slice you open!"

msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"Niedlich ...\n"
"\\.\\.Und jetzt werdet ihr alle sterben !"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"Cute...\n"
"\\.\\.And now you will all die!"

msgid ""
"\\>\\c[4]Mann\\c[0]\n"
"Neiiiiiiiii ..."
msgstr ""
"\\>\\c[4]Man\\c[0]\n"
"Noooooo..."

msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"STIEFVATER !!!!!!!!"
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"STEPFATHER!!!"

msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"NEIIIIIIIIIIIN !!!!"
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"NOOOOOOOO!!!"

msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"HARHAR !!!\n"
"\\.\\.Und jetzt bist du dran !!!"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"HARHAR!!!\n"
"\\.\\.And now it's your turn!!!"

msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"Bitte ...."
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"Please...."

msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"VAMPIR !!!!"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"VAMPIRE!!!"

msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Ich werde dich vernichten !!!"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"I'm going to kill you!!!"

msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"\\s[11]Vincent ...\n"
"Vincent Weynard !!!!"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"\\s[11]Vincent...\n"
"Vincent Weynard!!!"

msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Du hast dein letztes Opfer gehabt !"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"That was your last victim!"

msgid ""
"\\>\\c[5]Abraxas\\c[0]\n"
"ROAAAAAAR !!!!"
msgstr ""
"\\>\\c[5]Abraxas\\c[0]\n"
"ROAAAAAAR!!!"

msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Dich erwische ich auch noch !!!"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"I'll catch you sooner or later!!!"

msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Alles ok ?"
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Are you all right?"

msgid ""
"\\>\\c[8]Asgar\\c[0]\n"
"A \\..\\..\\.. a\\..\\..\\.. alles ok ????\n"
"\\.\\.MEINE STIEFELTERN WURDEN\n"
"GETÖTET !!!!"
msgstr ""
"\\>\\c[3]Asgar\\c[0]\n"
"A\\..\\..\\.. a\\..\\..\\.. all right!?\n"
"\\.\\.MY STEPPARENTS HAVE BEEN KILLED!!!"

msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Es tut mir leid, wäre ich doch bloß\n"
"früher gekommen ..."
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"I'm sorry, if only I had arrived\n"
"sooner..."

msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Gehe nach Asran, dort wird man\n"
"dir ein Zimmer geben, wenn du\n"
"ihnen sagst, dass ich dich schicke ..."
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Go to Asran, someone will give you a\n"
"room there if you tell them I sent\n"
"you..."

msgid ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"Es tut mir so leid ..."
msgstr ""
"\\>\\c[9]Vincent Weynard\\c[0]\n"
"I am so sorry..."

