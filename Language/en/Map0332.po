msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Merkwürdig, das hier ein\n"
"Brunnen steht ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Strange, that a well is here just in\n"
"the middle of nowhere like  this..."

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Wirklich seltsam ...\n"
"\\.\\.Mitten in der Wüste."
msgstr ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Truly peculiar...\n"
"\\.\\.In the middle of the desert."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Vielleicht ist er irgendwie\n"
"magisch oder so ?"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Perhaps it's magical, or something\n"
"of the sort?"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Vielleicht ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Maybe..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich schmeiß mal diesen\n"
"weißen Stein in den Brunnen ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"All right, I'm going to throw this\n"
"white stone into the well..."

