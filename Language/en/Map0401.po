msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Vollkommen versteinert !"
msgstr ""

msgid ""
"\\>\\c[5]Junger Mann:\\c[0]\n"
"Meine geliebte Jana !\n"
"\\.\\.Ohhh, wie konnte das nur passieren ..."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wow, ihr Gehirn ist über den\n"
"ganzen Boden verteilt!"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Tut mir leid wegen deiner Freundin,\n"
"wir konnten nichts mehr für sie tun !"
msgstr ""

msgid ""
"\\>\\c[5]Junger Mann:\\c[0]\n"
"\\s[4]...............\\s[0]"
msgstr ""

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Es tut uns so leid...."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Diese Statue hat ein riesen Loch\n"
"im Kopf !"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Saftig."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Das Loch in ihrem Kopf war wohl\n"
"ihr Ende ..."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wer bist du ?\n"
"\\.\\.WAS bist du ?\n"
"\\.\\.Und warum stehen hier überall"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"versteinerte Menschen ?"
msgstr ""

msgid ""
"\\>\\c[8]Hexe:\\c[0]\n"
"HARHAR !\n"
"\\.\\.Ich bin Ismaralda, die Gefürchtete !"
msgstr ""

msgid ""
"\\>\\c[8]Hexe:\\c[0]\n"
"Diese paar Bewohner habe ich\n"
"aus Spaß in Stein verwandelt.\n"
"\\.\\.Und schon bald wird auch der"
msgstr ""

msgid ""
"\\>\\c[8]Hexe:\\c[0]\n"
"Rest der Menschheit versteinert sein !\n"
"\\.\\.Ich werde sie alle verzaubern !\n"
"\\.\\.HARHARHAR !"
msgstr ""

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Du glaubst doch nicht wirklich,\n"
"dass du damit davonkommst?"
msgstr ""

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Tut mir leid für dich, aber das\n"
"werden wir verhindern !"
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Denn versteinerte Menschen\n"
"haben kein Blut !"
msgstr ""

msgid ""
"\\>\\c[8]Hexe:\\c[0]\n"
"Eure peinlichen Belange scheren mich\n"
"einen Dreck !\n"
"\\.\\.Ich werde euch jetzt auch versteinern !"
msgstr ""

msgid ""
"\\>\\c[8]Hexe:\\c[0]\n"
"Ihr widerliches Menschenpack !"
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Was für ein fataler Fehler, uns\n"
"als MENSCHEN einzuschätzen ..."
msgstr ""

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Keine Sorge, im deinem letzten\n"
"Augenblick wirst du die Wahrheit\n"
"erkennen ..."
msgstr ""

