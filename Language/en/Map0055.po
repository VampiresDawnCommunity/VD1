msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hier geht es nicht zum\n"
"Magierzimmer !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"This is not the way to the magic\n"
"chamber!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Es hat keinen Zweck jetzt zu\n"
"fliehen ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"There is no point in running away\n"
"now..."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wir müssen das Schloss\n"
"vom Ungeziefer befreien !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"We have to rid the castle of the\n"
"vermin!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Warte.\n"
"\\.\\.Lass uns zuerst ins Magierzimmer\n"
"gehen."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Not yet.\n"
"\\.\\.First, let's visit the Magic Chamber."

msgid ""
"\\>\\c[11]Untote Ritterin:\\c[0]\n"
"\\S[2]Alles ruhig, Gebieter."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Eine verschlossene Tür.\\.\\.\n"
"Da steht etwas..."
msgstr ""

msgid ""
"\\>\\c[8]Zwei neue Leben unter einem blutroten Mond.\n"
"Dort, wo das Leben und der Tod sich berühren,\n"
"lockt dich ein Flüstern aus deiner Vergangenheit\n"
"aus deinem Alptraum."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Dort, wo das Leben und der Tod sich\n"
"berühren\"\\.\\..\\.\\..?"
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wovon redest du? Da steht doch gar\n"
"nichts!\\. Was du hier wieder für eine\n"
"Aufregung um eine glorifizierte"
msgstr ""

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Abstellkammer machst."
msgstr ""

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Ich möchte nicht ausschließen, dass\n"
"du da tatsächlich etwas wahrnimmst,\n"
"aber für mich ist das auch bloß eine"
msgstr ""

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"gewöhnliche Tür.\\.\\.\n"
"Ich wundere mich, was dahinter sein\n"
"mag?"
msgstr ""

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Wenn du dir sicher bist, dass da etwas\n"
"dran ist, wird es wohl mit dir ganz\n"
"persönlich zusammenhängen..."
msgstr ""

