msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich muss zu Asgar !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I need to follow Asgar!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Verschlossen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Locked!"

msgid ""
"\\>\\c[5]Wache:\\c[0]\n"
"Haben wir dich, Bursche !!!"
msgstr ""
"\\>\\c[5]Guard:\\c[0]\n"
"Got you now, lad!!!"

msgid ""
"\\>\\c[5]Wache:\\c[0]\n"
"Wir verkünden hiermit die Todesstrafe,\n"
"für den Mord am Bürgermeister, seiner\n"
"Frau und der jungen Maria !"
msgstr ""
"\\>\\c[5]Guard:\\c[0]\n"
"We now sentence you to death for the murder of the\n"
"mayor, his wife, and the young Maria!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Falsch, ICH verkünde die Todesstrafe !\n"
"Für EUCH !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wrong, I sentence YOU to death! By my\n"
"hand!!!"

msgid ""
"\\>\\c[5]Wache:\\c[0]\n"
"Schnappen wir ihn uns !"
msgstr ""
"\\>\\c[5]Guard:\\c[0]\n"
"Get him!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ihr dummen Menschen ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"You foolish humans..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Jetzt muss ich zurück zum Schloss !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Now I need to get back to the\n"
"castle!!!"

