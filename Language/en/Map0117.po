#. ID 2, Page 2, Line 3, Pos (10,7)
#. ID 3, Page 2, Line 3, Pos (9,7)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich muss zu Asgar !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I need to follow Asgar!"

#. ID 19, Page 1, Line 3, Pos (15,10)
#. ID 20, Page 1, Line 3, Pos (16,10)
#. ID 26, Page 1, Line 3, Pos (3,10)
#. ID 27, Page 1, Line 3, Pos (4,10)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Verschlossen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Locked!"

#. ID 25, Page 1, Line 3, Pos (13,11)
msgid ""
"\\>\\c[5]Wache:\\c[0]\n"
"Haben wir dich, Bursche !!!"
msgstr ""
"\\>\\c[5]Guard:\\c[0]\n"
"Got you now, lad!!!"

#. ID 25, Page 1, Line 5, Pos (13,11)
msgid ""
"\\>\\c[5]Wache:\\c[0]\n"
"Wir verkünden hiermit die Todesstrafe,\n"
"für den Mord am Bürgermeister, seiner\n"
"Frau und der jungen Maria !"
msgstr ""
"\\>\\c[5]Guard:\\c[0]\n"
"We now sentence you to death for the murder of the\n"
"mayor, his wife, and the young Maria!"

#. ID 25, Page 1, Line 11, Pos (13,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Falsch, ICH verkünde die Todesstrafe !\n"
"Für EUCH !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wrong, I sentence YOU to death! By my\n"
"hand!!!"

#. ID 25, Page 1, Line 15, Pos (13,11)
msgid ""
"\\>\\c[5]Wache:\\c[0]\n"
"Schnappen wir ihn uns !"
msgstr ""
"\\>\\c[5]Guard:\\c[0]\n"
"Get him!"

#. ID 25, Page 1, Line 29, Pos (13,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ihr dummen Menschen ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"You foolish humans..."

#. ID 25, Page 1, Line 44, Pos (13,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Jetzt muss ich zurück zum Schloss !!!"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Now I need to get back to the\n"
"castle!!!"

