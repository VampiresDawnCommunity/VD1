msgstr ""
"\n"
"              \\_Weak Soul obtained!"

msgid ""
"\n"
"           Eine normale Seele erhalten !"
msgstr ""
"\n"
"              \\_Normal Soul obtained!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich muss noch den Rest umbringen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I need to kill the rest first!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich muss noch die Zeugen beseitigen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I need to dispose of the witnesses\n"
"first!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Es darf keine Zeugen geben !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"There must be no witnesses!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Die Zeugen müssen weg. "
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"The witnesses must die."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"ZEUGEN."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"WITNESSES."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ein Geist !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"A ghost!"

msgid ""
"\\>\\c[6]Geist:\\c[0]\n"
"Ahhhhh ...\n"
"\\.\\.Endlich ..."
msgstr ""
"\\>\\c[6]Ghost:\\c[0]\n"
"Ahhhhh...\n"
"\\.\\.At last..."

msgid ""
"\\>\\c[6]Geist:\\c[0]\n"
"Ich bin meiner jetzigen Existenz\n"
"müde ...\n"
"\\.\\.Keine Freude mehr ..."
msgstr ""
"\\>\\c[6]Ghost:\\c[0]\n"
"My current existence is tiring...\n"
"\\.\\.No joy left..."

msgid ""
"\\>\\c[6]Geist:\\c[0]\n"
"Endlich ist jemand gekommen,\n"
"dem ich meine Kräfte verleihen\n"
"kann ..."
msgstr ""
"\\>\\c[6]Ghost:\\c[0]\n"
"Finally, someone came whom I can give my remaining\n"
"strength..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Etwa ich ?"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Me?"

msgid ""
"\\>\\c[6]Geist:\\c[0]\n"
"Ich bin müde ...\n"
"\\.\\.Nimm meine Kräfte, und leb\n"
"wohl !"
msgstr ""
"\\>\\c[6]Ghost:\\c[0]\n"
"I'm tired...\n"
"\\.\\.Take my energy and farewell!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm ....\n"
"\\.\\.Na schön."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm...\n"
"\\.\\.All right."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm ...."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm ...."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Äh, warte mal ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Uh, wait a second..."

