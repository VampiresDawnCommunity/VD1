msgctxt "actors.name"
msgid "Castle"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"\\S[3]Erdeeeeeeee\\.\\.\n"
"Frische, weiche, nussige, schlammige,\n"
"lebendige, tiefenbraune, süße"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Erdeeeeee!\\.\\.\n"
"ICH WILL NIE WIEDER\n"
"AUFSTEHEN!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"BEGRABT MICH GLEICH HIER.\n"
"LEBENDIG! UNTER VIEL ERDE."
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"\\S[3]ES LEBE DIE LUFT! \\.\\.\n"
"SIE LÄSST UNS LEBEN, \n"
"SIE WEHT MIR DURCHS KLEID,"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"SIE LÄSST MICH SCHWEBEN,\n"
"SIE BRINGT MICH SO WEIT!\\.\\.\n"
"ES LEBE DIE LUFT!!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Ja, ich gebe es ganz offen zu: \\.\\.\n"
"ICH BIN WASSERSÜCHTIG."
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Darin zu baden, es zu trinken, es\n"
"mir übers Haar laufen zu lassen, \n"
"hinenizuhüpfen und darin zu "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"ertrinken\\..\\..\\..\\.\\.\n"
"WASSER IST MEINE WELT.\\.\\.\n"
"WASSER BEDECKT DIE WELT! "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Licht umgibt mich, umgibt uns alle.\\.\\.\n"
"Welche Lebewesen würden die \n"
"wärmespendende Liebe des Sonnenlichts"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"NICHT geniessen?!\\.\\.\n"
"Da würde mir wirklich NIEMAND\\.\\.\n"
"einfallen! \\.\\.Nope! \\.\\.NIEMAND!\\.\\."
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"\\S[2]Das Feuer in meiner Seele\n"
"treibt mich an! Es beflügelt mich, \n"
"nach Höherem zu streben und "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"\\S[2]mir nichts gefallen zu lassen!\\.\\.\n"
"ICH BIN STARK, UNABHÄNGIG,\n"
"WUNDERSCHÖN!!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Die Dunkelheit der Schatten ist\n"
"nur furchteinflößend, wenn wir unsere\n"
"dunkle Fantasie auf sie anwenden."
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Ansonsten ist sie ein wohlig-warmer\n"
"Schutz vor allem, das einem da draußen\n"
"WIRKLICH Angst macht. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"HIER:\\.\\. \n"
"Hier möchte ich die Elemente\n"
"sammeln! Bringt sie mir. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Ich muss NUR wissen, WO SIE\n"
"SIND. BITTE."
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir haben den Erdtempel gefunden.\n"
"Hier, das ist die Position. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"DER LIEBLICHE ERDTEMPEL.\n"
"WAS WÄRE DIE ERDE OHNE\\.\\.\\.\\.\n"
"ERDE?\\.\\. SAGT KEIN WORT MEHR!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Jetzt können wir gemeinsam hin!\\.\\.\n"
"Immer, jederzeit, Tag und Nacht.\\.\\.\n"
"Erde ist schön!!"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir haben den Feuertempel gefunden.\n"
"Hier, das ist die Position. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Der Feuertempel. WIRKLICH?\n"
"Er lässt unsere Seelen brennen! \\.\\.\n"
"UND DAS IST WAS GUTES! "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Und nun stehen uns seine Türen\n"
"offen! Wir können tanzen, zusammen,\n"
"in lodernden Flammen! "
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir haben den Lichttempel gefunden.\n"
"Hier, das ist die Position. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Mein Lieblingselement! \\.\\.\n"
"ICH LIEBE SIE ALLE.\\.\\.\n"
"Aber das Licht erfüllt mein Herz!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Liebt ihr nicht auch wirklich das \n"
"Sonnenlicht?\\.\\.\n"
"DAS MÜSST IHR EINFACH. "
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir haben den Lufttempel gefunden.\n"
"Hier, das ist die Position. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"\\S[5]Der Lufttempel!\\.\\.\n"
"Dann kann ich wieder \n"
"flieeeeeegeeeeeeeeenn!!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"\\S[5]Im Lufttempel können wir uns\n"
"gemeinsam frei fühlen wie die Vögel!\n"
"Lasst uns abheben! "
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir haben den Wassertempel gefunden.\n"
"Hier, das ist die Position. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Endlich, ENDLICH wieder Wasser!\\.\\.\n"
"Ich habe schon angefangen, wirklich\n"
"bedenklich zu riechen! MJAM!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Baden, Duschen, Tauchen! \\.\\.\n"
"Auch Göttinnen sollten auf ihre\n"
"Hygiene achten, JAWOHL! "
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir haben den Schattentempel gefunden.\n"
"Hier, das ist die Position. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Der Schattentempel.\\.\\.\\.\\.\n"
"Viele denken, der wäre böse\\.\\.\\.\\.\n"
"IST ER NICHT. IM SCHATTEN "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"KANN EIN JEDES POTENTIAL\n"
"ENTSTEHEN, UM INS LICHT ZU\n"
"TRETEN. SCHATTEN = POTENTIAL. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Und nun lasst uns im Schatten\n"
"verschwinden!\\.\\. Um uns zu verbergen, an\n"
"uns zu wachsen, unser verborgenes"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Potential zu ergründen!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"IHR HABT ES WIRKLICH \n"
"GESCHAFFT:\\.\\. Alle Schreine sind\n"
"in meiner Nähe! "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Nun weiß ich, warum ich hier bin:\\.\\.\n"
"Um euch die SCHÖNHEIT aller \n"
"Elemente zu lehren!!"
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Und damit ihr das TUN KÖNNT,\n"
"bekommt ihr von mir die KRAFT\n"
"DER ELEMENTE. "
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Vielen Dank."
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Dankt nicht mir. Dankt Mutter Natur,\n"
"die uns mit ihren WUNDERVOLLEN \n"
"Gaben reichlich beschenkte. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Und jetzt los, LOS.\n"
"Werft Steine hinein, und werdet\n"
"eins mit ALLEM. "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"Vergesst nicht: \\.\\.\n"
"Die Elemente können euch stärken,\n"
"jederzeit. Manchmal sind es nur "
msgstr ""

msgid ""
"\\>\\c[12]Elshiebietey:\\c[0]\n"
"ein paar Steinwürfe, die euch vom\n"
"SIEG über eure Gegner trennen.\n"
"Zeigt ihnen unsere Kraft! "
msgstr ""

