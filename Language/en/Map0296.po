msgid ""
"\\>\\c[6]Ladenbesitzerin:\\c[0]\n"
"Hallo !\n"
"\\.\\.Wir haben die besten Rüstungen\n"
"zu den niedrigsten Preisen !"
msgstr ""
"\\>\\c[6]Shopkeeper:\\c[0]\n"
"Hello!\n"
"\\.\\.We have the best armor for the lowest prices!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Na dann zeig mir mal, was du\n"
"alles verkaufst ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Well then, show me what you're\n"
"selling..."

msgid ""
"\\>\\c[6]Ladenbesitzerin:\\c[0]\n"
"AAAAAAAAAAAAAHHHH !!!\n"
"\\.\\.MÖRDER !!! \\.\\.MONSTER !!!"
msgstr ""
"\\>\\c[6]Shopkeeper:\\c[0]\n"
"AAAAAAAAAAAAAHHHH!!!\n"
"\\.\\.MURDERERS!!! \\.\\.MONSTERS!!!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Immer diese hysterischen Frauen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Always these hysteric women!"

msgid ""
"\\>\\c[6]Ladenbesitzer:\\c[0]\n"
"Hallo !\n"
"\\.\\.Schauen Sie sich ruhig hier um !\n"
"\\.\\.Ich habe viele gute Waffen !"
msgstr ""
"\\>\\c[6]Shopkeeper:\\c[0]\n"
"Hello!\n"
"\\.\\.Feel free to look around!\n"
"\\.\\.I have a lot of good weapons!"

msgid ""
"\\>\\c[6]Ladenbesitzer:\\c[0]\n"
"OH NEIN !!!\n"
"\\.\\.VAMPIRE !!!!"
msgstr ""
"\\>\\c[6]Shopkeeper:\\c[0]\n"
"OH, NO!!! \\.\\.VAMPIRES!!!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Immer die gleichen letzten Worte !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Always the same last words!"

