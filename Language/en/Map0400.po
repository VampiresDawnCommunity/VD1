msgctxt "actors.name"
msgid "Forest3"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Vollkommen versteinert !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Completely petrified!"

msgid ""
"\\>\\c[7]Junge Frau:\\c[0]\n"
"Oh, was ist passiert ?\n"
"\\.\\.Ich kann mich an nichts mehr\n"
"erinnern !"
msgstr ""
"\\>\\c[7]Young Woman:\\c[0]\n"
"Oh, what happened?\n"
"\\.\\.I can't remember anything!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Du bist von einer Hexe\n"
"versteinert worden !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"You were turned into stone by a witch!"

msgid ""
"\\>\\c[7]Junge Frau:\\c[0]\n"
"Wie bitte ?\n"
"\\.\\.Das ist doch nicht euer Ernst, oder ?\n"
"\\.\\.Versteinert ... ts, ts, so ein Blödsinn !"
msgstr ""
"\\>\\c[7]Young Woman:\\c[0]\n"
"What?\n"
"\\.\\.You can't be serious, right?\n"
"\\.\\.Turned into stone... tsts, what nonsense!"

msgid ""
"\\>\\c[7]Junge Frau:\\c[0]\n"
"AAAAAAAAHHHHHHH !!!\n"
"\\.\\.Ihr habt mein Kind getötet !!!!\n"
"\\.\\.HILFEEEEEE !!!"
msgstr ""
"\\>\\c[7]Young Woman:\\c[0]\n"
"AAAAAAAAHHHHHHH!!!\n"
"\\.\\.You killed my child!!!\n"
"\\.\\.HEEEEEEELP!!!"

msgid ""
"\\>\\c[7]Kleiner Junge:\\c[0]\n"
"Ich fühl mich so komisch ...\n"
"\\.\\.Irgendwas muss passiert sein."
msgstr ""
"\\>\\c[7]Little Boy:\\c[0]\n"
"I'm feeling so strange...\n"
"\\.\\.Something must have happened."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Eine Hexe hat euch alle versteinert."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"A witch turned you all into stone."

msgid ""
"\\>\\c[7]Kleiner Junge:\\c[0]\n"
"Und Ihr habt uns gerettet ?"
msgstr ""
"\\>\\c[7]Little Boy:\\c[0]\n"
"And you saved us?"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ja."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Yes."

msgid ""
"\\>\\c[7]Kleiner Junge:\\c[0]\n"
"Öh, danke !\n"
"\\.\\.Aber jetzt will ich spielen !"
msgstr ""
"\\>\\c[7]Little Boy:\\c[0]\n"
"Uh, thank you! \\.\\.But now I wanna play!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Ts, Kinder ..."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Tsts, kids..."

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Goldig."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Too precious."

msgid ""
"\\>\\c[7]Kleiner Junge:\\c[0]\n"
"WUAHHHHHHHHH !!!\n"
"\\.\\.MAMA !!!!"
msgstr ""
"\\>\\c[7]Little Boy:\\c[0]\n"
"WUAHHHHHHHHH!!! \\.\\.MOMMY!!!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Kinder sind doch etwas Feines ...\n"
"\\.\\.Elende Heulsusen !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Children are something nice...\n"
"\\.\\.Wretched crybabies!"

