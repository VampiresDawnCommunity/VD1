msgctxt "actors.name"
msgid "Linar"
msgstr ""

msgid ""
"\\>\\c[17]Desideria:\\c[0]\n"
"Neulich war ich in der Nähe von\n"
"Schloss Tranak und da waren so\n"
"komische Lichter im Wald nördlich."
msgstr ""
"\\>\\c[17]Desideria:\\c[0]\n"
"The other day I was near Tranak Castle\n"
"and there were strange lights in the\n"
"forest north of it."

msgid ""
"\\>\\c[17]Desideria:\\c[0]\n"
"Mein Vater meinte, dort könnten\n"
"Todesfeen sein!"
msgstr ""
"\\>\\c[17]Desideria:\\c[0]\n"
"My father told me there might be\n"
"banshees!"

msgid ""
"\\>\\c[17]Desideria:\\c[0]\n"
"\\.\\.Ich würde da nicht hingehen."
msgstr ""
"\\>\\c[17]Desideria:\\c[0]\n"
"\\.\\.I'd stay away from there if I \n"
"were you."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm..."

