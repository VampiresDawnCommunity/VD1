"What the..."

msgid ""
"\\>\\c[19]Fermin:\\c[0]\n"
"AROOOOOOOOO!!!"
msgstr ""
"\\c[19]Fermin:\\c[0]\n"
"AROOOOOOOOO!!!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Ein Werwolf!"
msgstr ""
"\\c[3]Asgar:\\c[0]\n"
"A werewolf!"

msgid ""
"\\>\\c[19]Fermin:\\c[0]\n"
"Grrrrr!\n"
"\\.\\.Iiiiihr?!\n"
"\\s[7]...\\s[0]"
msgstr ""
"\\c[19]Fermin:\\c[0]\n"
"Grrrrr!\n"
"\\.\\.Yoooouuu?!\n"
"\\s[7]...\\s[0]"

msgid ""
"\\>\\c[19]Fermin:\\c[0]\n"
"Wartet, was ...\\.\\. was passiert hier?"
msgstr ""
"\\c[19]Fermin:\\c[0]\n"
"Wait, what's...\\.\\. what's going on?"

msgid ""
"\\>\\c[19]Fermin:\\c[0]\n"
"Enya?"
msgstr ""
"\\c[19]Fermin:\\c[0]\n"
"Enya?"

msgid ""
"\\>\\c[19]Fermin:\\c[0]\n"
"ENYA!\\.\\. NEIN!"
msgstr ""
"\\c[19]Fermin:\\c[0]\n"
"ENYA!\\.\\. NO!"

msgid ""
"\\>\\c[19]Fermin:\\c[0]\n"
"Ich ...\\.\\. Ich hab das getan.\n"
"\\.\\.Es tut mir so leid!\n"
"\\.\\.Enya ..."
msgstr ""
"\\c[19]Fermin:\\c[0]\n"
"I...\\.\\. I did this.\n"
"\\.\\.I'm so sorry!\n"
"\\.\\.Enya..."

