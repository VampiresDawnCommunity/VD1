msgctxt "actors.name"
msgid "Forest5"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir sollten sie in Ruhe lassen."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"We should leave them be."

msgid ""
"\\>\\c[1]Guntra:\\c[0]\n"
"Ich Guntra, ich vermissen Golbar;\n"
"Golbar weg, aber Golbar gutester\n"
"Freund."
msgstr ""
"\\c[1]Guntra:\\c[0]\n"
"Me Guntra, me miss Golbar; Golbar\n"
"away, but Golbar goodest friend."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Golbar? Den haben wir schon mal\n"
"getroffen."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"Golbar? We've met him before."

msgid ""
"\\>\\c[1]Guntra:\\c[0]\n"
"JA!!!\\.\\. ICH GLÜCKLICH SEIN!"
msgstr ""
"\\c[1]Guntra:\\c[0]\n"
"YES!!!\\.\\. ME HAPPY!"

msgid ""
"\\>\\c[1]Guntra:\\c[0]\n"
"Du nehmen. Nehmen Steintafel und\n"
"bringen zu Golbar."
msgstr ""
"\\c[1]Guntra:\\c[0]\n"
"You take. Take stone tablet to\n"
"Golbar!"

msgid "\\>\\c[2]Valnar:\\c[0]"
msgstr ""

msgid ""
"In Ordnung.\n"
"Ich schlagen, du sterben!"
msgstr ""
"All right.\n"
"Me smash, you die!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Das machen wir."
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"We'll do it."

msgid ""
"\\>\\c[1]Guntra:\\c[0]\n"
"Ich warten."
msgstr ""
"\\c[1]Guntra:\\c[0]\n"
"Me wait."

msgid ""
"\\>\\c[1]Guntra:\\c[0]\n"
"WAAAA??\\.\\. ROOOOAR!"
msgstr ""
"\\c[1]Guntra:\\c[0]\n"
"WHAAAA??\\.\\. ROOOOAR!"

