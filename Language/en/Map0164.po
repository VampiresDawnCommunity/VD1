#. ID 153, Page 1, Line 9, Pos (3,20)
#. ID 153, Page 2, Line 10, Pos (3,20)
#. ID 154, Page 2, Line 12, Pos (29,16)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hallo !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hello!"

#. ID 150, Page 1, Line 11, Pos (31,14)
msgid ""
"\\>\\c[5]Junge Dame:\\c[0]\n"
"Hallo ...\n"
"\\.\\.Ich finde dieses Fest SOOOO\n"
"schön !"
msgstr ""
"\\>\\c[5]Young Woman:\\c[0]\n"
"Hello...\n"
"\\.\\.I think this festival is SOOOO lovely!"

#. ID 150, Page 1, Line 17, Pos (31,14)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich finds auch ok ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I think it's okay, too..."

#. ID 150, Page 1, Line 20, Pos (31,14)
msgid ""
"\\>\\c[5]Junge Dame:\\c[0]\n"
"Ok ? \\.\\.OK ????\n"
"\\.\\.Dieses Fest ist SUPEEEER !!!"
msgstr ""
"\\>\\c[5]Young Woman:\\c[0]\n"
"Okay? \\.\\.OKAY!?! \\.\\.This festival is FANTASTIIIC!!!"

#. ID 150, Page 1, Line 25, Pos (31,14)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ja, ja, schon gut !\n"
"\\.\\.Ich ... äh ... muss dann mal\n"
"gehen ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Yeah, yeah, all right!\n"
"\\.\\.I... uh... have to go..."

#. ID 150, Page 2, Line 15, Pos (31,14)
msgid ""
"\\>\\c[5]Junge Dame:\\c[0]\n"
"Verschwinde !\n"
"\\.\\.Ich will nichts mit dir\n"
"zu tun haben !"
msgstr ""
"\\>\\c[5]Young Woman:\\c[0]\n"
"Back off!\n"
"\\.\\.I don't want to have anything to do with you!"

#. ID 150, Page 2, Line 21, Pos (31,14)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hrmpf ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hrmpf..."

#. ID 151, Page 1, Line 13, Pos (23,24)
#. ID 151, Page 2, Line 13, Pos (23,24)
#. ID 152, Page 1, Line 12, Pos (37,18)
#. ID 152, Page 2, Line 13, Pos (37,18)
#. ID 153, Page 1, Line 12, Pos (3,20)
#. ID 153, Page 2, Line 13, Pos (3,20)
msgid ""
"\\>\\c[5]Clown:\\c[0]\n"
"Stört mich bitte nicht !\n"
"\\.\\.Ihr seht doch, dass ich mich\n"
"konzentrieren muss !"
msgstr ""
"\\>\\c[5]Clown:\\c[0]\n"
"Please don't bother me!\n"
"\\.\\.Can't you see I need to concentrate?"

#. ID 151, Page 1, Line 19, Pos (23,24)
#. ID 151, Page 2, Line 19, Pos (23,24)
#. ID 152, Page 1, Line 18, Pos (37,18)
#. ID 152, Page 2, Line 19, Pos (37,18)
#. ID 153, Page 1, Line 18, Pos (3,20)
#. ID 153, Page 2, Line 19, Pos (3,20)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Entschuldige ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Sorry..."

#. ID 154, Page 1, Line 7, Pos (29,16)
msgid ""
"\\>\\c[6]Mädchen:\\c[0]\n"
"Hm, was mach ich denn als\n"
"Nächstes ?"
msgstr ""
"\\>\\c[6]Girl:\\c[0]\n"
"Hm, what do I do next?"

#. ID 154, Page 1, Line 12, Pos (29,16)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Was hast du denn bisher\n"
"gemacht ?"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"What else have you done?"

#. ID 154, Page 1, Line 16, Pos (29,16)
msgid ""
"\\>\\c[6]Mädchen:\\c[0]\n"
"Bisher ?\n"
"\\.\\.Also ich fand das Armdrücken\n"
"gut, aber ich hatte keine Chance ..."
msgstr ""
"\\>\\c[6]Girl:\\c[0]\n"
"What else?\n"
"\\.\\.Well I liked the arm wrestling, but I stood no\n"
"chance..."

#. ID 154, Page 1, Line 22, Pos (29,16)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, da könnte ich mich mal\n"
"dran versuchen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, I could give it a try!"

#. ID 154, Page 1, Line 26, Pos (29,16)
msgid ""
"\\>\\c[6]Mädchen:\\c[0]\n"
"Mach das !\n"
"Das Zelt ist gleich dort hinten !"
msgstr ""
"\\>\\c[6]Girl:\\c[0]\n"
"Don't let me stop you! The tent is right over\n"
"there!"

#. ID 154, Page 2, Line 15, Pos (29,16)
msgid ""
"\\>\\c[6]Mädchen:\\c[0]\n"
"AHHHH !\n"
"\\.\\.Lasst mich in Ruhe !"
msgstr ""
"\\>\\c[6]Girl:\\c[0]\n"
"AHHHH! \\.\\.Leave me be!"

#. ID 155, Page 1, Line 7, Pos (23,26)
msgid ""
"\\>\\c[6]Mädchen:\\c[0]\n"
"Ich liebe Clowns !"
msgstr ""
"\\>\\c[6]Girl:\\c[0]\n"
"I love clowns!"

#. ID 155, Page 1, Line 11, Pos (23,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Er ist ganz nett ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"He's quite decent..."

#. ID 155, Page 1, Line 15, Pos (23,26)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Bäh, Clowns !\n"
"\\.\\.Die schmecken so nach\n"
"Schminke !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Bah, clowns! \\.\\.They taste like make-up!"

#. ID 155, Page 1, Line 20, Pos (23,26)
msgid ""
"\\>\\c[6]Mädchen:\\c[0]\n"
"Häh ?"
msgstr ""
"\\>\\c[6]Girl:\\c[0]\n"
"Huh?"

#. ID 155, Page 1, Line 24, Pos (23,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ach, mach dir keine Gedanken ...\n"
"\\.\\.Schau lieber dem Clown zu !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Oh, don't mind him...\n"
"\\.\\.Just keep watching the clown!"

#. ID 155, Page 1, Line 28, Pos (23,26)
msgid ""
"\\>\\c[6]Mädchen:\\c[0]\n"
"Ohhh ...\n"
"\\.\\.Er ist so gut !"
msgstr ""
"\\>\\c[6]Girl:\\c[0]\n"
"Ohhh... \\.\\.He is so good!"

#. ID 155, Page 1, Line 33, Pos (23,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Schmeckt nach Schminke\" ...\n"
"\\.\\.Hehe ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Taste like make-up\"... \\.\\.Hehe..."

#. ID 155, Page 1, Line 37, Pos (23,26)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Stimmt aber !\n"
"\\.\\.Probier doch selber !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"It's true! \\.\\.You should try one!"

#. ID 155, Page 1, Line 43, Pos (23,26)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Ehrlich, ihr zwei..."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Honestly, you two..."

#. ID 155, Page 2, Line 11, Pos (23,26)
msgid ""
"\\>\\c[6]Mädchen:\\c[0]\n"
"Haut ab !\n"
"\\.\\.Ich mag euch nicht !"
msgstr ""
"\\>\\c[6]Girl:\\c[0]\n"
"Back off! \\.\\.I don't like you!"

#. ID 155, Page 2, Line 16, Pos (23,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Pass bloß auf, kleines Fräulein !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Watch your tongue, young lady!"

#. ID 156, Page 1, Line 7, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Der Clown ist wirklich nicht\n"
"schlecht !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"The clown is really not bad!"

#. ID 156, Page 1, Line 12, Pos (24,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Naja, geht so !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Well, he's so-so!"

#. ID 156, Page 1, Line 17, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Huch, wo ist denn mein Sohn ?"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Oops, where did my son go?"

#. ID 156, Page 1, Line 21, Pos (24,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich hab ihn nicht gesehen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I haven't seen him!"

#. ID 156, Page 1, Line 24, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Wenn ihr ihn seht, bringt ihn bitte\n"
"zu mir !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"If you see him, please bring him here to me!"

#. ID 156, Page 1, Line 29, Pos (24,26)
#. Contains choice at line 3 (2 options)
#. ID 165, Page 1, Line 13, Pos (23,40)
#. Contains choice at line 3 (2 options)
#. ID 169, Page 1, Line 13, Pos (21,23)
#. Contains choice at line 3 (2 options)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ähm ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Uhm..."

#. ID 156, Page 1, Line 31, Pos (24,26)
#. Choice (2 options, embedded in a message)
msgid ""
"... natürlich !\n"
"... ganz sicher nicht !"
msgstr ""
"...of course!\n"
"...definitely not!"

#. ID 156, Page 1, Line 34, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Danke schön !\n"
"\\.\\.Er ist ein 7 Jahre alter Junge\n"
"mit braunen Haaren !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Thank you!\n"
"\\.\\.He is a seven-year-old boy with brown hair!"

#. ID 156, Page 1, Line 40, Pos (24,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir halten nach ihm Ausschau."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"We'll be looking out for him."

#. ID 156, Page 1, Line 47, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Hrmpf ...\n"
"\\.\\.Dann eben nicht !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Hrmpf... \\.\\.Then don't!"

#. ID 156, Page 2, Line 8, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Oh, vielen Dank !\n"
"\\.\\.Komm her du kleiner Racker !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Oh, thank you! \\.\\.Come here, you little rascal!"

#. ID 156, Page 2, Line 12, Pos (24,26)
msgid ""
"\\>\\c[7]Junge:\\c[0]\n"
"Papa, ich will Luftballon haben !"
msgstr ""
"\\>\\c[7]Boy:\\c[0]\n"
"Daddy, I want a balloon!"

#. ID 156, Page 2, Line 15, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Ja, ja ... nachher !\n"
"\\.\\.Jetzt schauen wir erstmal\n"
"dem Clown zu !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Yes, yes... later!\n"
"\\.\\.Now we're going to watch the clown!"

#. ID 156, Page 2, Line 20, Pos (24,26)
msgid ""
"\\>\\c[7]Junge:\\c[0]\n"
"Bäh ... Clown langweilig !"
msgstr ""
"\\>\\c[7]Boy:\\c[0]\n"
"Bah... the clown is boring!"

#. ID 156, Page 2, Line 23, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Warum müssen Kinder immer so\n"
"kompliziert sein ?"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Why do children always have to be so complicated?"

#. ID 156, Page 2, Line 27, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Aber vielen Dank nochmals !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Anyway, thanks again!"

#. ID 156, Page 2, Line 30, Pos (24,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Keine Ursache !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Don't mention it!"

#. ID 156, Page 2, Line 34, Pos (24,26)
msgid ""
"\\>\\c[7]Junge:\\c[0]\n"
"Wiedersehen, Miss! \\.\\.\n"
"Danke, dass Sie so lieb waren!"
msgstr ""
"\\>\\c[7]Boy:\\c[0]\n"
"Bye, Miss! \\.\\.Thanks for being so nice\n"
"to me!"

#. ID 156, Page 2, Line 38, Pos (24,26)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Bis bald! \\.\\.Sei ein guter Junge, ja?"
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Goodbye! \\.\\.Be a good boy, okay?"

#. ID 156, Page 2, Line 43, Pos (24,26)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Du bist so gut mit Kindern,\n"
"Aysha!"
msgstr ""
"\\>\\c[4]Alaine:\\c[0]\n"
"You're so good with children, Aysha!"

#. ID 156, Page 2, Line 48, Pos (24,26)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Danke! \\.\\.Magst du Kinder?"
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Thanks! \\.\\.Do you like children, Alaine?"

#. ID 156, Page 2, Line 52, Pos (24,26)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Meine Erinnerungen an die Vergangenheit \n"
"sind sehr verschwommen, aber ich glaube, \n"
"ich mochte sie sehr! Vor allem Babys!"
msgstr ""
"\\>\\c[4]Alaine:\\c[0]\n"
"My memories are very hazy when it\n"
"comes to my past, but I think I used\n"
"to love them. \\.\\.Especially babies!"

#. ID 156, Page 2, Line 57, Pos (24,26)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[4](Auch ich liebe Babys. \n"
"Vor allem, wenn ihr Blut noch-)"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[4](I, too, love babies. Especially\n"
"when their blood is-...)"

#. ID 156, Page 2, Line 62, Pos (24,26)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Wage es nicht, den Satz zu ende\n"
"zu denken, Asgar!"
msgstr ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Don't you even dare finish that line\n"
"of thought, Asgar!"

#. ID 156, Page 2, Line 66, Pos (24,26)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"uuuupps..."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Whoops..."

#. ID 156, Page 2, Line 80, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Denkt daran: 7 Jahre, braune Haare.\n"
"\\.\\.Ich muss jetzt weiter dem\n"
"Clown zugucken !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Remember: Seven years old , brown hair.\n"
"\\.\\.I have to continue watching the clown now!"

#. ID 156, Page 3, Line 7, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Dieser Clown ist einfach\n"
"zu gut !\n"
"\\.\\.Grandios !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"This clown is simply too good! \\.\\.Sublime!"

#. ID 156, Page 4, Line 11, Pos (24,26)
msgid ""
"\\>\\c[6]Mann:\\c[0]\n"
"Verschwindet !\n"
"\\.\\.Ihr seid mir zu gruselig !"
msgstr ""
"\\>\\c[6]Man:\\c[0]\n"
"Get away! \\.\\.You are too scary in my opinion!"

#. ID 156, Page 4, Line 15, Pos (24,26)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Soso ... zu gruselig !\n"
"\\.\\.Ich kann noch viel gruseliger\n"
"werden !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Well, well... too scary?\n"
"\\.\\.I can be even much scarier!"

#. ID 157, Page 1, Line 7, Pos (46,2)
msgid ""
"\\>\\c[6]Kleiner braunhaariger Junge:\\c[0]\n"
"Wäääh ...\n"
"\\.\\.Mir ist total langweilig hier !\n"
"\\.\\.Will nach Hause !"
msgstr ""
"\\>\\c[6]Brown-Haired Little Boy:\\c[0]\n"
"Waaah...\n"
"\\.\\.I'm totally bored here!\n"
"\\.\\.I wanna go home!"

#. ID 157, Page 1, Line 14, Pos (46,2)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Hallo, Kleiner!\n"
"\\.\\.Ganz allein hier?"
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Hello, there, little one!\n"
"\\.\\.Are you alone here?"

#. ID 157, Page 1, Line 21, Pos (46,2)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Bist du ganz allein hier?"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Are you here all on your own?"

#. ID 157, Page 1, Line 26, Pos (46,2)
msgid ""
"\\>\\c[6]Kleiner braunhaariger Junge:\\c[0]\n"
"Nein ...\n"
"\\.\\.Aba Papa kümmat sich nicht um\n"
"mich !"
msgstr ""
"\\>\\c[6]Brown-Haired Little Boy:\\c[0]\n"
"No... \\.\\.But Daddy doesn't look after me!"

#. ID 157, Page 1, Line 32, Pos (46,2)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ts, ts, wie gedankenlos manche\n"
"Eltern sind ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Tsts, how careless some parents are..."

#. ID 157, Page 2, Line 7, Pos (46,2)
msgid ""
"\\>\\c[6]Kleiner braunhaariger Junge:\\c[0]\n"
"Laaaaaaaaaaaangweilig !"
msgstr ""
"\\>\\c[6]Brown-Haired Little Boy:\\c[0]\n"
"Boooooooooooooooring!"

#. ID 157, Page 2, Line 11, Pos (46,2)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich hab mit deinem\n"
"Papa gesprochen !\n"
"\\.\\.Ich soll dich zu ihm bringen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I have talked to your father!\n"
"\\.\\.I'm supposed to take you to him!"

#. ID 157, Page 2, Line 16, Pos (46,2)
msgid ""
"\\>\\c[6]Kleiner braunhaariger Junge:\\c[0]\n"
"...\n"
"\\.\\.Na schön !\n"
"\\.\\.Ich komm mit ..."
msgstr ""
"\\>\\c[6]Brown-Haired Little Boy:\\c[0]\n"
"...\n"
"\\.\\.Fine!\n"
"\\.\\.I'll go with you..."

#. ID 157, Page 2, Line 22, Pos (46,2)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Hier, nimm meine Hand!\n"
"\\.\\.Dann bleiben wir zusammen."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Here, take my hand!\n"
"\\.\\.That way we won't get seperated."

#. ID 157, Page 2, Line 26, Pos (46,2)
msgid ""
"\\>\\c[6]Kleiner braunhaariger Junge:\\c[0]\n"
"Okay..."
msgstr ""
"\\>\\c[6]Brown-Haired Little Boy:\\c[0]\n"
"Okay..."

#. ID 157, Page 2, Line 32, Pos (46,2)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Fein ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Good..."

#. ID 157, Page 3, Line 7, Pos (46,2)
msgid ""
"\\>\\c[6]Kleiner braunhaariger Junge:\\c[0]\n"
"Will zu Papa !"
msgstr ""
"\\>\\c[6]Brown-Haired Little Boy:\\c[0]\n"
"I wanna go back to Daddy!"

#. ID 157, Page 3, Line 12, Pos (46,2)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Na na, hab Geduld.\n"
"\\.\\.Wir bringen dich zu deinem\n"
"Vater."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Now, now, don't be impatient.\n"
"\\.\\.We'll take you to your father."

#. ID 157, Page 3, Line 17, Pos (46,2)
msgid ""
"\\>\\c[6]Kleiner braunhaariger Junge:\\c[0]\n"
"Okay!"
msgstr ""
"\\>\\c[6]Brown-Haired Little Boy:\\c[0]\n"
"Okay!"

#. ID 157, Page 3, Line 22, Pos (46,2)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir gehen ja schon !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"We're going!"

#. ID 157, Page 4, Line 7, Pos (46,2)
#. ID 285, Page 2, Line 7, Pos (22,26)
msgid ""
"\\>\\c[6]Kleiner braunhaariger Junge:\\c[0]\n"
"Wäääääääääh !!!!\n"
"\\.\\.Hab Angst !"
msgstr ""
"\\>\\c[6]Brown-Haired Little Boy:\\c[0]\n"
"Waaaaaaaaah!!! \\.\\.I'm scared!"

#. ID 159, Page 1, Line 7, Pos (14,11)
msgid ""
"\\>\\c[7]Junge:\\c[0]\n"
"Boah, was für ein schönes\n"
"Fest !"
msgstr ""
"\\>\\c[7]Boy:\\c[0]\n"
"Wow, what a marvelous festival!"

