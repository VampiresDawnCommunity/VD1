msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Der Eingang ist von großen Felsen\n"
"blockiert!"
msgstr ""
"\\c[2]Valnar:\\c[0]\n"
"The entrance is blocked by large\n"
"rocks!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Verdammt heiß hier !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Damn hot in here!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Und es stinkt extrem nach\n"
"Schwefel !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"And it smells strongly like sulfur\n"
"here!"

msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Wir sollten gut aufpassen !"
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"This heat probably means trouble\n"
"somewhere down the line..."

msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Diese Hitze kann nur\n"
"Ungutes bedeuten..."
msgstr ""
"\\>\\c[4]Alaine:\\c[0]\n"
"We should be cautious!"

