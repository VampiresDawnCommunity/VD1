#. ID 20, Page 1, Line 7, Pos (5,8)
msgid ""
"\\>\\c[6]Musiker:\\c[0]\n"
"Stört mich bitte nicht, ich spiele\n"
"gerade !"
msgstr ""
"\\>\\c[6]Musician:\\c[0]\n"
"Please don't disturb me, I'm performing right now!"

#. ID 20, Page 2, Line 7, Pos (5,8)
msgid ""
"\\>\\c[6]Musiker:\\c[0]\n"
"AHHHHHHH !!!!\n"
"\\.\\.MONSTER !!!!"
msgstr ""
"\\>\\c[6]Musician:\\c[0]\n"
"AHHHHHHH!!! \\.\\.MONSTERS!!!"

#. ID 21, Page 1, Line 7, Pos (9,8)
msgid ""
"\\>\\c[8]Musiker:\\c[0]\n"
"Stört mich bitte nicht, ich spiele\n"
"gerade !"
msgstr ""
"\\>\\c[8]Musician:\\c[0]\n"
"Please don't disturb me, I'm performing right now!"

#. ID 21, Page 2, Line 7, Pos (9,8)
msgid ""
"\\>\\c[8]Musiker:\\c[0]\n"
"HILFE !!!\n"
"\\.\\.HIER SIND MÖRDER !!!"
msgstr ""
"\\>\\c[8]Musician:\\c[0]\n"
"HELP!!! \\.\\.THERE ARE MURDERERS HERE!!!"

#. ID 22, Page 1, Line 7, Pos (11,8)
msgid ""
"\\>\\c[9]Musiker:\\c[0]\n"
"Hey, ich spiele gerade !"
msgstr ""
"\\>\\c[9]Musician:\\c[0]\n"
"Hey, I'm performing right now!"

#. ID 22, Page 2, Line 7, Pos (11,8)
msgid ""
"\\>\\c[9]Musiker:\\c[0]\n"
"OH MEIN GOTT !!!\n"
"\\.\\.VAMPIRE !!!"
msgstr ""
"\\>\\c[9]Musician:\\c[0]\n"
"OH MY GOD!!! \\.\\.VAMPIRES!!!"

#. ID 22, Page 2, Line 11, Pos (11,8)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"OH MEIN GOTT ... \\.\\.MENSCHEN !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"OH MY GOD... \\.\\.HUMANS!"

#. ID 23, Page 1, Line 7, Pos (8,10)
#. Contains choice at line 3 (2 options)
msgid ""
"\\>\\c[10]Musiker:\\c[0]\n"
"Sollen wir andere Musik spielen?"
msgstr ""
"\\>\\c[10]Musician:\\c[0]\n"
"Shall we play another song?"

#. ID 23, Page 1, Line 9, Pos (8,10)
#. Choice (2 options, embedded in a message)
msgid ""
"Ja!\n"
"Nein."
msgstr ""
"Yes!\n"
"No."

#. ID 23, Page 1, Line 16, Pos (8,10)
#. ID 33, Page 1, Line 55, Pos (7,13)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Danke dir, Valnar...\n"
"Jetzt ist die Musik viel\n"
"besser."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Thank you Valnar,..\n"
"The music is much better now."

#. ID 23, Page 1, Line 22, Pos (8,10)
#. ID 33, Page 1, Line 61, Pos (7,13)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Für dich doch immer, Aysha. "
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"For you, always, Aysha."

#. ID 23, Page 1, Line 26, Pos (8,10)
#. ID 33, Page 1, Line 65, Pos (7,13)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
".........."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
".........."

#. ID 23, Page 2, Line 7, Pos (8,10)
msgid ""
"\\>\\c[10]Musiker:\\c[0]\n"
"NEIN !!!\n"
"\\.\\.HABT GNADE !!!"
msgstr ""
"\\>\\c[10]Musician:\\c[0]\n"
"NO!!! \\.\\.HAVE MERCY!!!"

#. ID 23, Page 2, Line 11, Pos (8,10)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Leider gibt es keinen Grund,\n"
"dich zu verschonen ..."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Sadly, there is no reason to spare\n"
"you..."

#. ID 23, Page 2, Line 15, Pos (8,10)
msgid ""
"\\>\\c[10]Musiker:\\c[0]\n"
"Ich habe Frau und Kinder !!!"
msgstr ""
"\\>\\c[10]Musician:\\c[0]\n"
"I have wife and kids!!!"

#. ID 23, Page 2, Line 18, Pos (8,10)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Das ist ja gut, dann ist mein\n"
"heutiges Abendessen ja auch\n"
"gesichtert !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"That's good, it means my dinner for\n"
"today is prepared!"

#. ID 24, Page 1, Line 7, Pos (10,14)
msgid ""
"\\>\\c[9]Mann:\\c[0]\n"
"Sie sind einfach super, findet ihr nicht ?"
msgstr ""
"\\>\\c[9]Man:\\c[0]\n"
"They are just great, don't you think?"

#. ID 24, Page 1, Line 11, Pos (10,14)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Naja, geht so."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Well, they're okay."

#. ID 24, Page 1, Line 14, Pos (10,14)
msgid ""
"\\>\\c[9]Mann:\\c[0]\n"
"Wenn ihr ein anderes Lied hören wollt,\n"
"dann schmeißt einfach 10 Filar in\n"
"die Schale links von der Treppe !"
msgstr ""
"\\>\\c[9]Man:\\c[0]\n"
"If you wish to hear some other song, just toss in\n"
"10 Filar into the pot next to the stairs!"

#. ID 24, Page 1, Line 18, Pos (10,14)
msgid ""
"\\>\\c[9]Mann:\\c[0]\n"
"Sie wechseln dann ihr Lied !"
msgstr ""
"\\>\\c[9]Man:\\c[0]\n"
"They will then play another song!"

#. ID 24, Page 2, Line 7, Pos (10,14)
msgid ""
"\\>\\c[9]Mann:\\c[0]\n"
"Ihr seid MONSTER !!!!\n"
"\\.\\.HILFEEEE !!!"
msgstr ""
"\\>\\c[9]Man:\\c[0]\n"
"You are MONSTERS!!! \\.\\.HEEEELP!!!"

#. ID 24, Page 2, Line 12, Pos (10,14)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Deine Einsicht kommt leider zu spät !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Sadly, your realization came too late!"

#. ID 25, Page 1, Line 7, Pos (6,14)
msgid ""
"\\>\\c[7]Tanzende Frau:\\c[0]\n"
"Oh, wie ich diese Musik liebe !\n"
"\\.\\.Sie kommen ursprünglich aus\n"
"Asdion."
msgstr ""
"\\>\\c[7]Dancing Woman:\\c[0]\n"
"Oh, how I love this music!\n"
"\\.\\.They're originally from Asdion."

#. ID 25, Page 1, Line 13, Pos (6,14)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Naja, sie sind aber nicht allzu\n"
"besonders."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Well, I don't think they're that\n"
"special."

#. ID 25, Page 1, Line 17, Pos (6,14)
msgid ""
"\\>\\c[7]Tanzende Frau:\\c[0]\n"
"Also ich find sie richtig gut !"
msgstr ""
"\\>\\c[7]Dancing Woman:\\c[0]\n"
"Well, I think they are really good!"

#. ID 25, Page 2, Line 7, Pos (6,14)
msgid ""
"\\>\\c[7]Frau:\\c[0]\n"
"AAAAAAAHHHHH !!!!\n"
"\\.\\.SO HELFT MIR DOCH !!!"
msgstr ""
"\\>\\c[7]Woman:\\c[0]\n"
"AAAAAAAHHHHH!!! \\.\\.SOMEBODY HELP ME!!!"

#. ID 25, Page 2, Line 12, Pos (6,14)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Keine Sorge, wir helfen dir doch.\n"
"\\.\\.Natürlich auf unsere Art !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Don't worry, we're going to help you.\n"
"\\.\\.In our own way of course!"

#. ID 27, Page 1, Line 7, Pos (19,22)
msgid ""
"\\>\\c[6]Barmann:\\c[0]\n"
"Hallo !\n"
"\\.\\.Setzt euch und genießt die Musik !"
msgstr ""
"\\>\\c[6]Barkeeper:\\c[0]\n"
"Hello! \\.\\.Take a seat and enjoy the music!"

#. ID 27, Page 1, Line 12, Pos (19,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Danke für das Angebot, aber wir\n"
"schauen uns nur ein bisschen um !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Thanks for the offer, but we're just\n"
"looking around!"

#. ID 27, Page 1, Line 16, Pos (19,22)
msgid ""
"\\>\\c[6]Barmann:\\c[0]\n"
"Macht das, solange ihr keine\n"
"Unruhe stiftet seid ihr hier willkommen !"
msgstr ""
"\\>\\c[6]Barkeeper:\\c[0]\n"
"Do that! So long as you don't cause any trouble,\n"
"you're welcome here!"

#. ID 27, Page 2, Line 7, Pos (19,22)
msgid ""
"\\>\\c[6]Barmann:\\c[0]\n"
"VERSCHWINDET SOFORT,\n"
"IHR MONSTER !!!!!"
msgstr ""
"\\>\\c[6]Barkeeper:\\c[0]\n"
"LEAVE THIS PLACE AT ONCE, YOU MONSTERS!!!"

#. ID 27, Page 2, Line 12, Pos (19,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Für diese Gastunfreundlichkeit\n"
"wirst du sterben !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"For such bad hospitality, you're going\n"
"to die!"

#. ID 28, Page 1, Line 7, Pos (18,18)
msgid ""
"\\>\\c[9]Blonder Mann:\\c[0]\n"
"Was wollt ihr von mir ?"
msgstr ""
"\\>\\c[9]Blond Man:\\c[0]\n"
"What do you want from me?"

#. ID 28, Page 1, Line 11, Pos (18,18)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Eigentlich nichts ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Nothing, actually..."

#. ID 28, Page 1, Line 14, Pos (18,18)
msgid ""
"\\>\\c[9]Mann:\\c[0]\n"
"Nichts ?\n"
"\\.\\.Warum belästigt ihr mich dann ?"
msgstr ""
"\\>\\c[9]Blond Man:\\c[0]\n"
"Nothing? \\.\\.Then why are you bothering me?"

#. ID 28, Page 1, Line 19, Pos (18,18)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Mangel an Kommunikation ?"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Lack of conversations?"

#. ID 28, Page 1, Line 22, Pos (18,18)
msgid ""
"\\>\\c[9]Blonder Mann:\\c[0]\n"
"So wie ihr ausseht, ist das ja auch kein Wunder !"
msgstr ""
"\\>\\c[9]Blond Man:\\c[0]\n"
"The way you look, it's no mystery why!"

#. ID 28, Page 1, Line 26, Pos (18,18)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[4](Es würde mir viel Freude\n"
"bereiten, diesem Blödmann die\n"
"Eingeweide rauszureißen !)"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[4](It would be my pleasure to tear out\n"
"the guts of this dumbass!)"

#. ID 28, Page 2, Line 7, Pos (18,18)
msgid ""
"\\>\\c[9]Blonder Mann:\\c[0]\n"
"VERDAMMT !\n"
"\\.\\.Ihr seid VAMPIRE !!!"
msgstr ""
"\\>\\c[9]Blond Man:\\c[0]\n"
"DAMN IT! \\.\\.You are VAMPIRES!!!"

#. ID 28, Page 2, Line 12, Pos (18,18)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Und so wie ich das sehe, bist du\n"
"ein MENSCH !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"And as far as I can see, you are a\n"
"HUMAN!"

#. ID 28, Page 2, Line 15, Pos (18,18)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Bleibt nur noch die Frage,\n"
"wer von uns gleich sterben wird !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"The only question is who of us is\n"
"going to die now!"

#. ID 29, Page 1, Line 7, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Wenn ich nur nicht immer meinen\n"
"Geldbeutel verlegen würde !"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"If only I didn't misplace my wallet all the time!"

#. ID 29, Page 1, Line 10, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Jetzt bin ich hier und habe kein Geld\n"
"bei mir ! \\.\\.Verflixt !"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Now I'm here and I don't have any money with me!\n"
"\\.\\.Damn it!"

#. ID 29, Page 1, Line 13, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Hm, ihr seht wie ein paar ehrliche\n"
"Leute aus !\n"
"\\.\\.Würdet ihr mein Geld holen ?"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Hm, you look like some honest people!\n"
"\\.\\.Would you mind bringing me my money?"

#. ID 29, Page 1, Line 17, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Ich habe das Geld im Schrank\n"
"gelassen. Es sind 400 Filar.\n"
"\\.\\.Ihr würdet von mir 100 Filar bekommen."
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"I left the money at home on my shelf. It's 400\n"
"Filar.\n"
"\\.\\.I would give you 100 Filar."

#. ID 29, Page 1, Line 23, Pos (6,22)
#. Contains choice at line 3 (2 options)
#. ID 29, Page 2, Line 12, Pos (6,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm..."

#. ID 29, Page 1, Line 25, Pos (6,22)
#. Choice (2 options, embedded in a message)
msgid ""
"... gut, wir holen das Geld !\n"
"... nein, wir haben keine Zeit."
msgstr ""
"...all right, we'll get it!\n"
"...no, we don't have time."

#. ID 29, Page 1, Line 28, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Oh, ich danke euch !\n"
"\\.\\.Mein Haus liegt im Nordosten der\n"
"Stadt !"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Oh, thank you!\n"
"\\.\\.My house is in the northeast of this town!"

#. ID 29, Page 1, Line 32, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Der Geldbeutel sollte in einem der Regale\n"
"im Erdgeschoss liegen."
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"The wallet should be on one of the shelves on the \n"
"first floor."

#. ID 29, Page 1, Line 37, Pos (6,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Gut, wir werden ihn suchen."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Good, we'll search for it."

#. ID 29, Page 1, Line 43, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Oh wie schade.\n"
"\\.\\.Nun ja, da muss mir der Barkeeper\n"
"einen Aufschub gewähren ..."
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Oh, too bad.\n"
"\\.\\.Well, I'll just ask the barkeeper if I can pay\n"
"later, then..."

#. ID 29, Page 2, Line 7, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Habt ihr mein Geld ?"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Did you get my money?"

#. ID 29, Page 2, Line 14, Pos (6,22)
#. Choice (3 options)
msgid ""
"... ja, wir haben es !\n"
"... es war kein Geldbeutel da !\n"
"... nein, leider noch nicht."
msgstr ""
"...yes, we did!\n"
"...there was no wallet!\n"
"...not yet, I'm afraid."

#. ID 29, Page 2, Line 17, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Oh, ich danke euch !"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Oh, thank you!"

#. ID 29, Page 2, Line 19, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Und ich hatte euch ja 100 Filar\n"
"versprochen, doch da ihr so ehrliche\n"
"Leute seid, bekommt ihr 150 Filar !"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"I promised you 100 Filar, but since you are such\n"
"honest folks, I'll give you 150 Filar!"

#. ID 29, Page 2, Line 23, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Folglich bekomme ich noch die\n"
"restlichen 250 Filar von euch !"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Therefore, I get the remaining 250 Filar from you!"

#. ID 29, Page 2, Line 28, Pos (6,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hier, bitte !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Here you go!"

#. ID 29, Page 2, Line 41, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Es war keiner da ?\n"
"\\.\\.Habt ihr auch in allen Regalen\n"
"geschaut ?"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"It wasn't there? \\.\\.Did you check all shelves?"

#. ID 29, Page 2, Line 48, Pos (6,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ja, natürlich."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Yes, of course."

#. ID 29, Page 2, Line 51, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"So was aber auch ...\n"
"\\.\\.Wo könnte ich es nur hingelegt\n"
"haben ?"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Well, would you look at that...\n"
"\\.\\.Where could I possibly have put it?"

#. ID 29, Page 2, Line 55, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Trotzdem danke, dass ihr für\n"
"mich nachgeschaut habt !\n"
"\\.\\.Leider kann ich euch so natürlich"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Still, thank you for looking!\n"
"\\.\\.Unfortunately, I can't give you any money since I\n"
"don't have any myself!"

#. ID 29, Page 2, Line 59, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"auch kein Geld geben, da ich im Moment\n"
"gar keins habe !"
msgstr "<easyrpg:delete_page>"

#. ID 29, Page 2, Line 64, Pos (6,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Schon gut, hat ja keine Umstände\n"
"gemacht !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"It's all right, it wasn't any trouble\n"
"at all!"

#. ID 29, Page 2, Line 75, Pos (6,22)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"War das jetzt dringend nötig?"
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Was that really necessary?"

#. ID 29, Page 2, Line 83, Pos (6,22)
#. ID 29, Page 2, Line 96, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Beeilt euch bitte, ich muss bald meine\n"
"Rechnung bezahlen ..."
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Please hurry, I soon have to pay the bill..."

#. ID 29, Page 2, Line 92, Pos (6,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Nein, wir sind noch nicht dazu\n"
"gekommen."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"No, we didn't get around to doing it\n"
"yet."

#. ID 29, Page 3, Line 7, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"Lasst mich bitte überlegen, was ich\n"
"nun als Nächstes machen werde."
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"Now please let me think what I'm going to do next."

#. ID 29, Page 4, Line 7, Pos (6,22)
msgid ""
"\\>\\c[9]Ältere Frau:\\c[0]\n"
"VAMPIRE !!!\n"
"\\.\\.IHR SEID WAHRHAFTIG VAMPIRE !!!"
msgstr ""
"\\>\\c[9]Older Woman:\\c[0]\n"
"VAMPIRES!!! \\.\\.YOU REALLY ARE VAMPIRES!!!"

#. ID 29, Page 4, Line 12, Pos (6,22)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Das hast du sehr gut erkannt.\n"
"\\.\\.Doch leider wird dir dieses Wissen\n"
"nicht viel bringen !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"You are quite the smart one.\n"
"\\.\\.But unfortunately, that knowledge\n"
"isn't going to help you much!"

#. ID 33, Page 1, Line 3, Pos (7,13)
#. Contains choice at line 3 (2 options)
msgid ""
"\n"
"               10 Filar reinwerfen ?"
msgstr ""
"\n"
"                \\_Toss in 10 Filar?"

#. ID 33, Page 1, Line 5, Pos (7,13)
#. Choice (2 options, embedded in a message)
msgid ""
"                     Ja\n"
"                    Nein"
msgstr ""
"Yes\n"
"No"

#. ID 33, Page 1, Line 9, Pos (7,13)
msgid ""
"\\>\\c[9]Musiker:\\c[0]\n"
"Danke vielmals !"
msgstr ""
"\\>\\c[9]Musician:\\c[0]\n"
"Thank you very much!"

#. ID 35, Page 1, Line 3, Pos (19,7)
#. ID 36, Page 1, Line 3, Pos (18,7)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hier stehen nur einige Bücher über\n"
"die Einnahmen und Ausgaben dieses\n"
"Pubs ..."
msgstr ""
