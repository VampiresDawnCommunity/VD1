msgid ""
"\\>\\c[7]Sir Kedrigh:\\c[0]\n"
"Huch, was machen Sie denn hier ?\n"
"Verschwinden Sie ! Ich habe Sie\n"
"nicht hereingebeten !"
msgstr ""
"\\>\\c[7]Sir Kedrigh:\\c[0]\n"
"Huh, what are you doing here? Get out of here, I\n"
"didn't invite you in!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[4](Arrogantes Menschenpack !)"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\s[4](Arrogant human scum!)"

