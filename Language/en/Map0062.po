#. ID 20, Page 1, Line 2, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rom:\\c[0]\n"
"\\S[3]Pssssssshhhhhhtt...\\.\\.\\.\n"
"Rim, Rim!"
msgstr ""

#. ID 20, Page 1, Line 6, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rim:\\c[0]\n"
"\\S[3]Pssssssshhhhhhtt...\\.\\.\\.\n"
"Rom, Rom!"
msgstr ""

#. ID 20, Page 1, Line 10, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rom:\\c[0]\n"
"\\S[3]Der Vampir vor uns mag \n"
"das Mädchen gern, Rim!"
msgstr ""

#. ID 20, Page 1, Line 14, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rim:\\c[0]\n"
"\\S[3]Er mag es mehr, als er \n"
"zugeben möchte, Rom!"
msgstr ""

#. ID 20, Page 1, Line 19, Pos (67,79)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\S[2](Was zum....)"
msgstr ""

#. ID 20, Page 1, Line 22, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rom:\\c[0]\n"
"\\S[3]Er macht sich sorgen, Rim, Rim!"
msgstr ""

#. ID 20, Page 1, Line 25, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rim:\\c[0]\n"
"\\S[3]Wie albern, Rom, Rom!\n"
"Dabei kann nur er uns hören,\n"
"nicht seine Kameraden!"
msgstr ""

#. ID 20, Page 1, Line 31, Pos (67,79)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\S[2](.........)"
msgstr ""

#. ID 20, Page 1, Line 34, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rom:\\c[0]\n"
"\\S[3]Rim, Rim. Lass ihm uns verraten,\n"
"dass seine Zuneigung zu ihr \n"
"schon \\V[15] beträgt!"
msgstr ""

#. ID 20, Page 1, Line 40, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rim:\\c[0]\n"
"\\S[3]Wollen wir ihm verraten, \n"
"dass seine Zuneigung zu seiner \n"
"Freundin \\V[16] beträgt, Rom, Rom?"
msgstr ""

#. ID 20, Page 1, Line 45, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rom:\\c[0]\n"
"\\S[3]Für wen wird er sich wohl\n"
"entscheiden, Rim, Rim?"
msgstr ""

#. ID 20, Page 1, Line 49, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rim:\\c[0]\n"
"\\S[3]Für wen nur, Rom, Rom?"
msgstr ""

#. ID 20, Page 1, Line 54, Pos (67,79)
msgid ""
"\\>\\c[6]Dämonin Rim:\\c[0]\n"
"\\S[3]Ob er sie am Ende wohl\n"
"erobert, Rom, Rom?"
msgstr ""

#. ID 23, Page 1, Line 3, Pos (59,85)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ein Gästebuch ...\n"
"mal sehen wer hier so alles\n"
"das Schloss besucht hat:"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"A guestbook...\n"
"Let's see who has visited the castle:"

#. ID 23, Page 1, Line 7, Pos (59,85)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Raziel - ich \\.kam, \\.sah \\.und hab gut\n"
"gegessen ...\""
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Raziel - I \\.came, \\.saw, \\.and ate\n"
"well..."

#. ID 23, Page 1, Line 10, Pos (59,85)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Shriekback - Ich muss schon sagen,\n"
"hier wird reichlich gespammt !\""
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Shriekback - I have to say, there's a\n"
"lot of spamming here!\""

#. ID 23, Page 1, Line 13, Pos (59,85)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Coldy - Hey Shriekback, das wär\n"
"doch ein guter Ort zum du weißt\n"
"schon was !\""
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Coldy - Hey Shriekback, this would be\n"
"a great place for... you know!\""

#. ID 23, Page 1, Line 17, Pos (59,85)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Sehr, sehr seltsam, was hier\n"
"für Leute waren ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Very, very bizarre the kind of people\n"
"who have been here..."

#. ID 23, Page 1, Line 23, Pos (59,85)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Um es höflich auszudrücken..."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"To put it mildly..."

#. ID 23, Page 1, Line 30, Pos (59,85)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hier stehen ein paar volle\n"
"Gästebücher. Aber ich habe\n"
"keinen besonderen Gast entdeckt."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Here are some filled out guestbooks.\n"
"But I don't see any notable guest."

#. ID 29, Page 1, Line 2, Pos (62,94)
msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"\\S[3]Hmmm, duu..."
msgstr ""

#. ID 29, Page 1, Line 5, Pos (62,94)
msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"\\S[3]Du scheinst ein unheimlich \n"
"reiner Vampir zu sein... bedeuten\n"
"dir die Menschen so viel?"
msgstr ""

#. ID 29, Page 1, Line 10, Pos (62,94)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Danke fürs Kompliment.\\|\n"
"Es geht mir einfach darum, \n"
"mit Vernunft und Würde zu handeln."
msgstr ""

#. ID 29, Page 1, Line 17, Pos (62,94)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Wohl gesprochen, Valnar!"
msgstr ""

#. ID 29, Page 1, Line 20, Pos (62,94)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\S[5](Mir wird schwindelig,\n"
"HARHARHARHAR)"
msgstr ""

#. ID 29, Page 1, Line 28, Pos (62,94)
msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"\\S[3]Du scheinst weder besonders\n"
"gut noch abscheulich böse zu\n"
"sein... seltsamer Vampir."
msgstr ""

#. ID 29, Page 1, Line 33, Pos (62,94)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich war noch nie für Extreme.\\|\n"
"Es geht mir einfach darum, \n"
"stets flexibel zu bleiben."
msgstr ""

#. ID 29, Page 1, Line 38, Pos (62,94)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Eine respektable Einstellung,\n"
"Valnar."
msgstr ""

#. ID 29, Page 1, Line 44, Pos (62,94)
msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"\\S[3]Deine Aura ist schwarz wie die \n"
"Nacht. Es ist mir eine Ehre, einem so \n"
"edlen Vampir zu dienen."
msgstr ""

#. ID 29, Page 1, Line 49, Pos (62,94)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Das höre ich gerne.\\|\n"
"Letztendlich weiß ich \n"
"einfach, meine Macht zu nutzen."
msgstr ""

#. ID 29, Page 1, Line 56, Pos (62,94)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Ein bisschen Vernunft wäre aber\n"
"auch nicht schlecht..."
msgstr ""

#. ID 29, Page 1, Line 60, Pos (62,94)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"\\S[5](Du machst dich Valnar,\n"
"hehehe...)"
msgstr ""

#. ID 30, Page 1, Line 2, Pos (67,94)
msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"So wie Jinnall und unsere Späher\n"
"es berichten, gibt es in dieser Welt\n"
"keine nennenswerten Großmachten"
msgstr ""

#. ID 30, Page 1, Line 6, Pos (67,94)
msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"abgesehen von unserer und der \n"
"Armee dieses Königs Gerald. \n"
"Unsere Wache ist also eher "
msgstr ""

#. ID 30, Page 1, Line 10, Pos (67,94)
msgid ""
"\\>\\c[6]Ghoul:\\c[0]\n"
"theoretisch, man weiß ja nie."
msgstr ""

#. ID 30, Page 1, Line 14, Pos (67,94)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Ganz genau. Um den König haben\n"
"wir uns zwar gekümmert, da kommt\n"
"niemand mehr, aber irgendein"
msgstr ""

#. ID 30, Page 1, Line 18, Pos (67,94)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wahnsinniger wird sich schon\n"
"mal wieder hierherverirren, \n"
"und dann SIND WIR DA."
msgstr ""

#. ID 30, Page 1, Line 26, Pos (67,94)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Ganz genau. Wer weiß, ob dieser \n"
"verdammte Gerald nicht nochmal\n"
"einen Spähtrupp entsendet."
msgstr ""

#. ID 30, Page 1, Line 30, Pos (67,94)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Nicht, dass es mich stören würde.\n"
"Diesmal sind wir VORBEREITET."
msgstr ""

#. ID 33, Page 1, Line 3, Pos (63,97)
#. ID 37, Page 1, Line 3, Pos (66,99)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Eine Ritterleiche...\n"
"Hier liegen noch ein paar\n"
"Dinge herum."
msgstr ""

#. ID 34, Page 1, Line 2, Pos (65,95)
msgid ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Meister, Meister !!!!!\n"
"\\.\\.Endlich seid Ihr zurück !!!"
msgstr ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Master, Master!\n"
"\\.\\.Finally, you have returned!!!"

#. ID 34, Page 1, Line 7, Pos (65,95)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Was ist los, Ronak ?"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"What's going on, Ronak?"

#. ID 34, Page 1, Line 10, Pos (65,95)
msgid ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Ritter, Meister !!!\n"
"\\.\\.Ritter sind gekommen, und\n"
"stürmen gerade das Schloss !"
msgstr ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Knights, Master!!!\n"
"\\.\\.Knights appeared and are now storming\n"
"the castle!"

#. ID 34, Page 1, Line 14, Pos (65,95)
msgid ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Es sind zu viele, Meister !\n"
"\\.\\.Ich konnte nur ein paar aufhalten !"
msgstr ""
"\\>\\c[4]Ronak:\\c[0]\n"
"They are too many, Master!\n"
"\\.\\.I could only stop a few!"

#. ID 34, Page 1, Line 19, Pos (65,95)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Diese widerlichen Insekten !!!\n"
"\\.\\.Kommt ihr beiden, wir kümmern\n"
"uns um dieses Problem !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Those nasty insects!!!\n"
"\\.\\.Come, you two, we will take care of\n"
"this problem!"

#. ID 34, Page 1, Line 25, Pos (65,95)
msgid ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Sicher !"
msgstr ""
"\\>\\c[4]Alaine:\\c[0]\n"
"Certainly!"

#. ID 34, Page 1, Line 29, Pos (65,95)
#. Contains choice at line 3 (2 options)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Aber ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"But..."

#. ID 34, Page 1, Line 31, Pos (65,95)
#. Choice (2 options, embedded in a message)
msgid ""
"... das sind alles Menschen !\n"
"... wir sollten sie zerfetzen !"
msgstr ""
"...they are all humans!\n"
"...we should tear them apart!"

#. ID 34, Page 1, Line 36, Pos (65,95)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wenn du jetzt rumzickst, wirst du\n"
"genauso brutal sterben wie die\n"
"Ritter !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"If you start bitching now, you will\n"
"brutally die just like the knights!"

#. ID 34, Page 1, Line 42, Pos (65,95)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Äh...\n"
"\\.\\.Schon gut, gehen wir !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Uhh...\n"
"\\.\\.All right, let's go!"

#. ID 34, Page 1, Line 50, Pos (65,95)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Und ob wir das machen !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"You bet we will!"

#. ID 34, Page 1, Line 55, Pos (65,95)
msgid ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Ich bleibe hier und passe auf, das\n"
"niemand mehr reinkommt !"
msgstr ""
"\\>\\c[4]Ronak:\\c[0]\n"
"I stay here and make sure nobody else\n"
"gets in!"

#. ID 34, Page 1, Line 60, Pos (65,95)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Nein, du passt auf dass niemand\n"
"FLÜCHTET !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"No, you make sure that nobody ESCAPES!"

#. ID 34, Page 1, Line 63, Pos (65,95)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Für diese Freveltat müssen sie\n"
"alle qualvoll sterben !!!"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"For this insolence, they must all die\n"
"in agony!!!"

#. ID 34, Page 1, Line 68, Pos (65,95)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Na, dann suchen wir mal den\n"
"Anführer dieser Ritter !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Well, let's look for the leader of\n"
"those knights!"

#. ID 34, Page 1, Line 72, Pos (65,95)
msgid ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Hier habt Ihr noch einige Tränke,\n"
"Meister !"
msgstr ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Here are some potions for you, Master!"

#. ID 34, Page 2, Line 2, Pos (65,95)
msgid ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Der Anführer lebt noch, Meister !\n"
"\\.\\.Ich kann seine Anwesenheit\n"
"spüren !"
msgstr ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Their leader is still alive, Master!\n"
"\\.\\.I can sense his presence!"

#. ID 34, Page 2, Line 8, Pos (65,95)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Glaubst du etwa, ich weiß das nicht ?\n"
"Pass gefälligst weiter auf, dass\n"
"niemand entkommt !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Do you really think I don't know that?\n"
"You just make sure none of the knights\n"
"escape!"

#. ID 34, Page 2, Line 13, Pos (65,95)
msgid ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Natürlich, Meister !!!"
msgstr ""
"\\>\\c[4]Ronak:\\c[0]\n"
"Of course, Master!"

#. ID 40, Page 1, Line 6, Pos (64,100)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hier geht es nicht zum\n"
"Magierzimmer !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"This is not the way to the magic\n"
"chamber!"

#. ID 40, Page 1, Line 13, Pos (64,100)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Es hat keinen Zweck jetzt zu\n"
"fliehen ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"There is no point in running away\n"
"now..."

#. ID 40, Page 1, Line 27, Pos (64,100)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Wir müssen das Schloss\n"
"vom Ungeziefer befreien !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"We have to rid the castle of the\n"
"vermin!"

#. ID 40, Page 1, Line 39, Pos (64,100)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Warte.\n"
"\\.\\.Lass uns zuerst ins Magierzimmer\n"
"gehen."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Not yet.\n"
"\\.\\.First, let's visit the Magic Chamber."

#. ID 48, Page 1, Line 4, Pos (58,5)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hier steht ein Buch mit dem\n"
"Titel \"Die Legende der sieben\n"
"Lebensgeister\"."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Here is a book with the title:\n"
"\"The Legend of the Seven Spirits of\n"
"Life\"."

#. ID 48, Page 1, Line 8, Pos (58,5)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"Die sieben Lebensgeister existieren,\n"
"doch sind sie versteckt vor allem\n"
"Lebendigen. Sie sind der Ursprung."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\"The seven spirits of life do exist,\n"
"but they are hidden from all living\n"
"beings. They are the origin of all."

#. ID 48, Page 1, Line 12, Pos (58,5)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Kein Sterblicher hat sie je gesehen,\n"
"und doch sind wir sicher, dass sie\n"
"existieren."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"No mortal has ever seen one, yet we\n"
"are sure of their existence. Should\n"
"there ever be someone who manages to"

#. ID 48, Page 1, Line 16, Pos (58,5)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Sollte es jemals jemanden gelingen,\n"
"alle sieben Lebensgeister zu finden,\n"
"wird ihm unendliches Leben sicher"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"find all seven spirits, they will be\n"
"granted eternal life, and they will be\n"
"able to build a pleasant life on earth"

#. ID 48, Page 1, Line 20, Pos (58,5)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"sein, und er wird sich über alle\n"
"Jahrhunderte dieser Erde ein\n"
"schönes Leben aufbauen können.\""
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"over the centuries.\""

#. ID 48, Page 1, Line 24, Pos (58,5)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"\\.\\.Unendliches Leben ...\n"
"\\.\\.Das habe ich bereits jetzt.\n"
"\\.\\.Und doch ist es eine Qual !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Eternal life...\n"
"\\.\\.I already have that.\n"
"\\.\\.Yet it's anguish!"

#. ID 48, Page 1, Line 31, Pos (58,5)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"..."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"..."

#. ID 48, Page 1, Line 38, Pos (58,5)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hier stehen einige alte Bücher\n"
"über eine Legende der sieben\n"
"Lebensgeister ..."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Here are some old books about the \n"
"legend of the seven spirits of life..."

#. ID 48, Page 1, Line 42, Pos (58,5)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Alles Humbug !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hokum, all of it!"

#. ID 50, Page 1, Line 4, Pos (65,5)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Die Bücher in diesem Regal\n"
"sind zu mottenzerfressen,\n"
"um sie lesen zu können."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"The books on this shelf are too mothy\n"
"to be readable."

#. ID 50, Page 1, Line 11, Pos (65,5)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Du solltest dich wirklich besser\n"
"um deine Bücher kümmern..."
msgstr ""
"\\>\\c[2]Aysha:\\c[0]\n"
"You really should take better care of\n"
"your books..."

#. ID 50, Page 1, Line 15, Pos (65,5)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Und du hättest das Schloss sehen\n"
"sollen, BEVOR ich es übernahm.\n"
"Gruselig..."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"And you really should have seen the\n"
"castle before I took over if you think\n"
"this is bad. \\.\\.It was a complete mess."

#. ID 50, Page 1, Line 24, Pos (65,5)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hier stehen einige alte Bücher,\n"
"die leider bereits von Motten\n"
"zerfressen wurden."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Here are some old books that sadly\n"
"have already been eaten by moths."

#. ID 50, Page 1, Line 28, Pos (65,5)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hm ...\n"
"\\.\\.Wieso leider ?\n"
"\\.\\.War eh alles unwichtig !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hm...\n"
"\\.\\.Why am I saying sadly?\n"
"\\.\\.It was all unimportant anyway!"

#. ID 54, Page 1, Line 2, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Mein Name ist Knorkhof,\n"
"Gebieter. Diesen Namen solltet \n"
"Ihr Euch merken, denn ich bin "
msgstr ""

#. ID 54, Page 1, Line 6, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"ein nützlicher Diener, der\n"
"nützlichste von allen! Hier,\n"
"das habe ich im Schloss gefunden."
msgstr ""

#. ID 54, Page 1, Line 14, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Nicht schlecht, Knorkhof,\n"
"ich muss schon sagen."
msgstr ""

#. ID 54, Page 1, Line 18, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Wie ich sagte, Gebieter - \n"
"Der Nützlichste! Kommt wieder\n"
"zu mir, wenn ihr die 2. und 3. Welle "
msgstr ""

#. ID 54, Page 1, Line 22, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"beschworen hat, dann werde ich\n"
"sicher mehr gefunden haben!"
msgstr ""

#. ID 54, Page 1, Line 26, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich behalte das im Hinterkopf."
msgstr ""

#. ID 54, Page 2, Line 2, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Euer NÜTZLICHER Diener\n"
"Knorkhof hält auch heute die \n"
"Gänge frei!"
msgstr ""

#. ID 54, Page 2, Line 7, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir wissen das sehr zu\n"
"schätzen."
msgstr ""

#. ID 54, Page 3, Line 2, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Mein Name ist Knorkhof,\n"
"und ich weiß GENAU, warum Ihr\n"
"hier seid, Gebieter."
msgstr ""

#. ID 54, Page 3, Line 7, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"So?\\.\\.\n"
"Bitte erleuchte mich, Knorkhof. "
msgstr ""

#. ID 54, Page 3, Line 11, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Ihr wisst mittlerweile um die\n"
"zuverlässigen Qualitäten Eures\n"
"NÜTZLICHSTEN Dieners!!"
msgstr ""

#. ID 54, Page 3, Line 16, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Du hast mich erwischt."
msgstr ""

#. ID 54, Page 3, Line 19, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Euer Weg war nicht umsonst,\n"
"Gebieter. GANZ IM GEGENTEIL."
msgstr ""

#. ID 54, Page 3, Line 26, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Es ist immer eine Freude,\n"
"unseren nützlichsten Diener\n"
"aufzusuchen."
msgstr ""

#. ID 54, Page 3, Line 31, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Die Freude ist ganz EURERSEITS,\n"
"Gebieter! Und so freue ich, KNORKHOF,\n"
"mich über Euren nächsten Besuch nach"
msgstr ""

#. ID 54, Page 3, Line 35, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"der 3. Welle an Beschöwrungen!\n"
"Bleibt knorkig!"
msgstr ""

#. ID 54, Page 4, Line 2, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Euer NÜTZLICHER Diener\n"
"Knorkhof hält auch heute die \n"
"Straßen sauber!"
msgstr ""

#. ID 54, Page 4, Line 7, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Vielen Dank."
msgstr ""

#. ID 54, Page 5, Line 2, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Mein Name ist Knorkhof,\n"
"und ich bin bereit, zum Mitarbeiter\n"
"des Monats zu werden."
msgstr ""

#. ID 54, Page 5, Line 7, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Der Titel ist dir hiermit verliehen.\\.\\.\n"
"Gut gemacht."
msgstr ""

#. ID 54, Page 5, Line 11, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Oh Ihr EHRT mich Gebieter,\n"
"Ihr ahnt nicht, was es mir bedeutet,\n"
"ENDLICH mal anerkannt zu "
msgstr ""

#. ID 54, Page 5, Line 15, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"werden.\\.\\.\n"
"Siehst du das, Mama?\\.\\.\n"
"SIEHST DU ES, DU ALTE-"
msgstr ""

#. ID 54, Page 5, Line 20, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ähm Knorkhof, ich möchte dich \n"
"nicht unterbrechen, aber konntest\n"
"du wieder etwas Nützliches finden?"
msgstr ""

#. ID 54, Page 5, Line 25, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Ihr FRAGT, Gebieter?!\\.\\.\n"
"Aber bitte!!"
msgstr ""

#. ID 54, Page 5, Line 32, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Das ist ein ziemlicher \n"
"Batzen Geld, vielen Dank!"
msgstr ""

#. ID 54, Page 5, Line 36, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Es war mir wirklich ein Vergnügen,\n"
"Gebieter. Und weil ihr uns nun alle\n"
"hierher beschworen habt, möchte"
msgstr ""

#. ID 54, Page 5, Line 40, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"ich Euch noch ein besonderes Danke\n"
"überreichen. Handgemacht. War eine\n"
"Knochenarbeit."
msgstr ""

#. ID 54, Page 5, Line 48, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich werde ihn immer tragen \n"
"und den Finger nie wieder waschen,\n"
"Knorkhof."
msgstr ""

#. ID 54, Page 5, Line 53, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Das ist aber artig von Euch,\n"
"Gebieter.\\.\\.\n"
"Und vergesst nicht:"
msgstr ""

#. ID 54, Page 5, Line 57, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Euer NÜTZLICHSTER \n"
"Diener Knorkhof ist immer \n"
"hier, um für Ordnung zu sorgen!"
msgstr ""

#. ID 54, Page 6, Line 2, Pos (68,11)
msgid ""
"\\>\\c[6]Skelettgardist:\\c[0]\n"
"Euer nützlichster Diener Knorkhof\n"
"hält auch heute unter Einsatz \n"
"seiner Wirbelsäule die Gänge frei."
msgstr ""

#. ID 54, Page 6, Line 7, Pos (68,11)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wir wüssten nicht, was wir ohne dich\n"
"täten, Knorkhof. Wirklich nicht."
msgstr ""

#. ID 55, Page 1, Line 2, Pos (60,9)
msgid ""
"\\>\\c[4]Skelett:\\c[0]\n"
"Gebieter, Ronak hat schon wieder\n"
"die Menschen in Eurem\n"
"Gefängnis gequält !"
msgstr ""
"\\>\\c[4]Skeleton:\\c[0]\n"
"My lord, Ronak has been torturing the\n"
"humans in your prison again!"

#. ID 55, Page 1, Line 7, Pos (60,9)
msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Na, immerhin dazu ist er noch\n"
"fähig !!!!"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Well, at least he still seems to be\n"
"capable of that much!"

#. ID 55, Page 2, Line 2, Pos (60,9)
msgid ""
"\\>\\c[4]Skelett:\\c[0]\n"
"Herr, Ihr solltet Euch weiterbilden !\n"
"\\.\\.Die Bücher in diesem Schloss sind\n"
"dazu bestens geeignet !"
msgstr ""
"\\>\\c[4]Skeleton:\\c[0]\n"
"Sire, you should study more!\n"
"\\.\\.The books in this castle are ideally\n"
"suited for this purpose!"

#. ID 55, Page 2, Line 8, Pos (60,9)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Danke für den Ratschlag !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Thanks for the advice!"

#. ID 55, Page 2, Line 11, Pos (60,9)
msgid ""
"\\>\\c[4]Skelett:\\c[0]\n"
"Aber natürlich, Herr !"
msgstr ""
"\\>\\c[4]Skeleton:\\c[0]\n"
"But of course, Sire!"

