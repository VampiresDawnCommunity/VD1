msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, ein großes K ist da an\n"
"die Wand gemalt ...\n"
"\\.\\.merkwürdig !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, a large \"C\" has been painted on\n"
"the wall...\n"
"\\.\\.Strange!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, ein großes A ist da an\n"
"die Wand gemalt ...\n"
"\\.\\.merkwürdig !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, a large \"A\" has been painted on\n"
"the wall...\n"
"\\.\\.Strange!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Wer bist du ?"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Who are you?"

msgid ""
"\\>\\c[8]Sektenmitglied:\\c[0]\n"
"Stellt mir keine Fragen.\n"
"\\.\\.Ich kann spüren, dass ihr\n"
"Vampire seid ..."
msgstr ""
"\\>\\c[8]Cult Member:\\c[0]\n"
"Don't ask me questions.\n"
"\\.\\.I can sense that you are vampires..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Jemand in einem Versteck der\n"
"Vampire hat gemeint, wir sollten\n"
"hier die \"Kinder der Apokalypse\""
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Someone in a vampire hideout said we\n"
"should look for the \"Children of the\n"
"Apocalypse\" here..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"suchen ...\n"
"\\.\\.Ich nehme an, das K und das A\n"
"stehen wohl dafür ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I assume the C and the A stand for\n"
"that..."

msgid ""
"\\>\\c[8]Sektenmitglied:\\c[0]\n"
"Tretet ein ..."
msgstr ""
"\\>\\c[8]Cult Member:\\c[0]\n"
"Go inside..."

