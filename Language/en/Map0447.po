msgid ""
"\\>\\c[2]Dr. Jarn:\\c[0]\n"
"Bis bald, Aysha. \\.\\.Ich wünsche dir nur \n"
"das Beste. \\.\\. Es war schön, dich wieder\n"
"zu sehen. Ich dachte wirklich, "
msgstr ""

#. ID 69, Page 2, Line 167, Pos (41,27)
msgid ""
"\\>\\c[2]Dr. Jarn:\\c[0]\n"
"ich hätte dich für immer verloren."
msgstr ""

#. ID 69, Page 2, Line 171, Pos (41,27)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Vielen Dank, Dr. Jarn. \\.\\.\n"
"Ich hoffe, wir sehen uns bald\n"
"wieder."
msgstr ""

#. ID 69, Page 2, Line 181, Pos (41,27)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Valnar! \\.\\.Hör sofort auf."
msgstr ""

#. ID 69, Page 2, Line 185, Pos (41,27)
#. ID 69, Page 2, Line 195, Pos (41,27)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Sorry..."
msgstr ""

#. ID 69, Page 2, Line 191, Pos (41,27)
msgid ""
"\\>\\c[2]Aysha:\\c[0]\n"
"Na!!"
msgstr ""

