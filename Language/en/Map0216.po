#. ID 119, Page 1, Line 3, Pos (46,27)
#. ID 120, Page 1, Line 3, Pos (42,17)
#. ID 121, Page 1, Line 3, Pos (42,6)
#. ID 122, Page 1, Line 3, Pos (43,9)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Leer !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Empty!"

#. ID 112, Page 1, Line 4, Pos (18,25)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hier liegt ein Wundverband !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Here is a bandage!"

#. ID 112, Page 2, Line 3, Pos (18,25)
msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Nichts weiter ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Nothing else here..."

