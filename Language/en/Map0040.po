msgctxt "actors.name"
msgid "RegalSchloss"
msgstr ""

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hier stehen einige unwichtige\n"
"Bücher ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Here are some uninteresting books..."

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, hier hinten ist irgendwas ..."
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, something's in the back here..."

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Das lässt du besser sein, sonst werde\n"
"ich ungemütlich !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"You better not touch that or I become\n"
"far less pleasant!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Schon gut !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"All right!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, und was hat der Hebel jetzt\n"
"bewirkt ?"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Hm, and what did the switch do?"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Aha !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Aha!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Was ist denn das ?\n"
"\\.\\.Der Schalter wurde betätigt ?\n"
"\\.\\.So, so ... Valnar !"
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"What's this?\n"
"\\.\\.The switch was activated?\n"
"\\.\\.Well, well... Valnar!"

msgid ""
"\\>\\c[2]Valnar:\\c[0]\n"
"Ich habe den Schalter schon\n"
"betätigt !"
msgstr ""
"\\>\\c[2]Valnar:\\c[0]\n"
"I already activated the switch!"

msgid ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Hier ist ein Schalter für ein kleines\n"
"Geheimnis. Aber den brauche ich\n"
"im Moment noch nicht."
msgstr ""
"\\>\\c[3]Asgar:\\c[0]\n"
"Here is a switch for a small secret\n"
"stash. But I don't need it right now."

